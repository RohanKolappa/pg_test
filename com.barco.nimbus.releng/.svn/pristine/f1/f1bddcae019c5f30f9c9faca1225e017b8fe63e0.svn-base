# location of the output
buckminster.output.root       /tmp/buckminster/com.barco.nimbus/out
# location of temporary files
buckminster.temp.root         /tmp/buckminster/com.barco.nimbus/tmp
# don't build source bundles
cbi.include.source            false
# at build time, fill the a.b.c.qualifier with the svn revision number of the project
qualifier.replacement.*=generator:lastRevision
# configure product.export to produce linux native launcher
target.os=linux
target.ws=gtk
target.arch=x86_64
