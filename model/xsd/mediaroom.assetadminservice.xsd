	<xs:element name="ExportImportFileListItem" type="ExportImportFileListItemType" />
	<xs:complexType name="ExportImportFileListItemType">
		<xs:sequence>
			<xs:element name="Internal" type="InternalType" />
			<xs:element name="External" type="ExternalType" />
			<xs:element minOccurs="0" maxOccurs="unbounded" name="Exception" type="ErrorType" />
		</xs:sequence>
		<xs:attribute name="type" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="Import" />
					<xs:enumeration value="Export" />
					<xs:enumeration value="Offload" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		
		<!-- 
			QUEUE_STATUS_COMPLETED = -1000;
			QUEUE_STATUS_ERROR = -10;
			QUEUE_STATUS_RUNNING = 1;
			QUEUE_STATUS_ERROR_RESUME = 10;
			QUEUE_STATUS_NOT_STARTED = 1000;
		 -->	    
 		<xs:attribute name="queue_state" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="-1000" />
					<xs:enumeration value="-10" />
					<xs:enumeration value="1" />
					<xs:enumeration value="10" />
					<xs:enumeration value="1000" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>		
		<!-- 
			export:	state = queued, ready, export_getDMSBlobPath, export_db, export_dbFileTransfer, export_encode, export_fileTransfer, export_cleanupTempFiles , completed, ready
			import: state = queued, ready, import_getDMSBlobPath, import_fileTransfer, import_decode, import_dbFileTransfer, import_db,  import_cleanupTempFiles , completed, ready
			on error = go to ready and process next in queue 
		-->
		<xs:attribute name="state" use="required">
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="ready" />
					<xs:enumeration value="queued" />
					<!-- export -->
					<xs:enumeration value="export_getDMSBlobPath" />
					<xs:enumeration value="export_db" />
					<xs:enumeration value="export_dbFileTransfer" />
					<xs:enumeration value="export_encode" />
					<xs:enumeration value="export_fileTransfer" />
					<xs:enumeration value="export_cleanupTempFiles" />
					<xs:enumeration value="completed" />
					<!-- import -->
					<xs:enumeration value="import_getDMSBlobPath" />
					<xs:enumeration value="import_fileTransfer" />
					<xs:enumeration value="import_decode" />
					<xs:enumeration value="import_dbFileTransfer" />
					<xs:enumeration value="import_db" />
					<xs:enumeration value="import_cleanupTempFiles" />
					<xs:enumeration value="completed" />
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="format" type="FormatObjectType"/> 
		<xs:attribute name="dateCompleted" type="xs:long" />
		<xs:attribute name="dateCreated" type="xs:string" />
		<xs:attribute name="dateUpdated" type="xs:long"  />
		<xs:attribute name="msJID" type="xs:string" />
		<xs:attribute name="ownerJID" type="xs:string" use="required" />
		<xs:attribute name="priority" type="xs:int" use="required" />
		<xs:attribute name="state_description" type="xs:string" />
		<xs:attribute name="errorCount" type="xs:long"/>
		<xs:attribute name="action" type="xs:string"/>
		<xs:attribute name="blobSize" type="xs:long"/>
		
		<xs:attribute name="serviceVer" type="xs:double" use="required" />
	    <xs:attribute fixed="1.8" name="Ver" type="xs:double" use="required" />
	    <xs:attribute fixed="1" name="Rev" type="xs:int" use="required" />
		<xs:attributeGroup ref="AutoNID" />
	</xs:complexType>
