										<xs:sequence>
											<xs:any />
										</xs:sequence>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
							<xs:attribute name="whereXPath" type="xs:string" />
							<xs:attribute name="selectFormat" type="NativeObjectSelectFormatType" />
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>		

 	<!-- GetObject Response -->
	<xs:element name="GetObjectResponseData" type="GetObjectResponseDataType" />
	<xs:complexType name="GetObjectResponseDataType">
		<xs:sequence>
			<xs:element name="GetObjectResponse">
				<xs:complexType>
					<xs:complexContent mixed="false">
						<xs:extension base="GetObjectResponseType">
							<xs:sequence>
								<xs:any />
							</xs:sequence>
							<xs:attribute name="type" type="ObjectListType" use="required" />
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>	

