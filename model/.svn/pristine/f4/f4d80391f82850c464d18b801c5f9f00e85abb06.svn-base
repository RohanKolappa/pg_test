     <!--  GetObject request -->
	<xs:element name="GetObjectRequest" type="GetObjectRequestDataType" />
	<xs:complexType name="GetObjectRequestDataType">
		<xs:sequence>
			<xs:element name="GetObjectRequest">
				<xs:complexType>
					<xs:complexContent mixed="false">
						<xs:extension base="GetObjectBaseType">
							<xs:sequence>
