	    <DstPort id="" portNID="default.mediastreamdstportresourcelist.
	    msdprl_1d9c4ced-5168-4d4d-bf7e-8b0011f676da" portTitle=""/>
	</View>
  ]]>
          </programlisting>
        </example>
          
