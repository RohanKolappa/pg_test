        <example>
          <title>Get Profile Request</title>
          <programlisting>
            <![CDATA[
	 <GetProfile startFrom="1" countToFetch="100" sortBy="" 
	        sortOrder=""
			selectFormat="BASIC/EXTENDED" 
			whereNID="" 
			whereTextSubStr="" 
			whereStreamType="" 
			whereTagExpr=""/>
           ]]>         
          </programlisting>
          
          <itemizedlist>
		   <listitem>
				<para>
					<parameter>sortBy="NID/title/streamType/bandwidth/multicast/avOption/enableKbm"</parameter>					
				</para>
			</listitem>			
		</itemizedlist>
