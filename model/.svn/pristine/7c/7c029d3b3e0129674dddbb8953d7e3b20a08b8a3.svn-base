  <!-- SourceEvent 
  	Values for the Error eventCode
  	public static final int END_OF_MEDIA = 12;
  	public static final int RELAY_SOURCE_UNAVAILABLE = 14;
  	public static final int RELAY_SOURCE_DISCONNECTED = 15;   
  	public static final int MEDIA_ERROR = 204;
  	public static final int UNKNOWN_EVENT = 100;
  	
  	Values for the Info eventCode
  	public static final int RELAY_SOURCE_STARTED = 10;
  	public static final int STREAM_START_TC = 11;
  	public static final int SOURCE_UNAVAILABLE = 13;
  -->
  <xs:complexType name="SourceEventDataType">
    <xs:sequence>
      <xs:element name="action">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="SourceEvent" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="eventCode" type="xs:int" />
      <xs:element name="streamNID" type="xs:string" />
      <xs:element name="startTC" type="xs:string" />
      <xs:element name="sourceEventActionData" type="xs:anyType" />
      <xs:element name="streamNIDList" type="xs:anyType" />
      <xs:element name="status" type="xs:int" />
    </xs:sequence>
  </xs:complexType>

  <!-- DestEvent  
  	Values for the Error eventCode
  	public static final int MEDIA_STORE_ERROR = 103; 
  	public static final int NO_AVAILABLE_DISK_SPACE = 9001;
  	
  	Values for the Info eventCode
  	//Destination Event Codes
  	public static final int RECORDING_STARTED = 101;
  	public static final int RECORDING_STOPPED = 102;
  	
  	destEventDoc.append(	"<"+event+"TimeStamp>" + timeStamp + "</"+event+"TimeStamp>");
  	destEventDoc.append(	"<"+event+"WallClock>" + wallclock + "</"+event+"WallClock>");
  	
  -->
  <xs:complexType name="DestEventDataType">
    <xs:sequence>
      <xs:element name="action">
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="DestEvent" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element name="streamNID" type="xs:string" />
      <xs:element name="eventCode" type="xs:int" />
      <xs:element name="StartedTimeStamp" type="xs:double" />
      <xs:element name="StartedWallClock" type="xs:double" />
      <xs:element name="StopTimeStamp" type="xs:double" />
      <xs:element name="StopWallClock" type="xs:double" />
      <xs:element name="destinationEventActionData" type="xs:anyType" />
      <xs:element name="status" type="xs:int" />
    </xs:sequence>
  </xs:complexType>
