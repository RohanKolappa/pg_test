	<chapter>
		<title>Set Operations</title>
		<section>
			<title>Set Service Client Display Configuration Request</title>
			<para>
				Sets the display configuration parameters for given service client.
			</para>
			<example>
				<title>Set Service Client Display Configuration Request</title>
				<programlisting>
            <![CDATA[
		<SetServiceClientDisplayConfig byNID=''>
			<IPVSClientDisplayConfig>
				<!-- check the unmanned client display config-->
			</IPVSClientDisplayConfig>
		</SetServiceClientDisplayConfig>
            ]]>
          </programlisting>
			</example>
			<example>
				<title>Set Service Client Display Configuration Response</title>
				<programlisting>
            <![CDATA[
		<SetServiceClientDisplayConfig byNID=''>
			<IPVSClientDisplayConfig>
			 <!-- check the unmanned client display config-->
			</IPVSClientDisplayConfig>
		</SetServiceClientDisplayConfig>
		    ]]>
          </programlisting>
			</example>
		</section>
		<section>
			<title>Set Service Client Display Status Request</title>
			<para>
				Sets the display status parameters for given service client.
			</para>
			<example>
				<title>Set Service Client Display Status Request</title>
				<programlisting>
            <![CDATA[
		<SetServiceClientDisplayStatus byNID=''>
			<IPVSClientDisplayStatus>
				<!-- check the unmanned client display status-->
			</IPVSClientDisplayStatus>
		</SetServiceClientDisplayStatus>
            ]]>
          </programlisting>
			</example>
			<example>
				<title>Set Service Client Display Status Response</title>
				<programlisting>
            <![CDATA[
		<SetServiceClientDisplayStatus byNID=''>
			<IPVSClientDisplayStatus>
			 <!-- check the unmanned client display status-->
			</IPVSClientDisplayStatus>
		</SetServiceClientDisplayStatus>
		    ]]>
          </programlisting>
			</example>
		</section>
	</chapter>

