         	<listitem>
				<para>
					<parameter>byNID</parameter>
					<optional>Required</optional>
				</para>
			</listitem>
			<listitem>
				<para>
					<parameter>byName</parameter>
					<optional>Optional</optional>
				</para>
			</listitem>
			</itemizedlist>
			<example>
				<title>Delete Report Request</title>
				<programlisting>
	            <![CDATA[
		      	<DeleteReport byNID='' byName='' />
	       ]]>
		    </programlisting>			
			</example>
			<example>
				<title>Delete Report Response</title>
				<programlisting>
	            <![CDATA[
		   	 	<DeleteReport/>	
	         ]]>
	          </programlisting>
			</example>
		</section>	
		
