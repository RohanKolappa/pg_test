		<section>
			<title>User Object</title>
			<para> This section explain list of User object API calls</para>
			<section>
				<title>AddUser</title>
				<para>Add User in database</para>
				<example>
					<title>Add User Request</title>
					<itemizedlist>
						<listitem>
							<para>
								<parameter>fullName</parameter>
								<optional>Required</optional>
							</para>
						</listitem>
						<listitem>
							<para>
								<parameter>loginName</parameter>
								<optional>Required</optional>
							</para>
						</listitem>
						<listitem>
							<para>
								<parameter>password</parameter>
								<optional>Required</optional>
							</para>
						</listitem>
						<listitem>
							<para>
								<parameter>email</parameter>
								<optional>Required</optional>
							</para>
						</listitem>
						<listitem>
							<para>
								<parameter>Groups</parameter>
								<optional>Optional</optional>
							</para>
						</listitem>
						<listitem>
							<para>
								<parameter>UserPrivatData</parameter>
								<optional>Optional</optional>
							</para>
						</listitem>
					</itemizedlist>
					<programlisting>
			            <![CDATA[
			<AddUser fullName='' loginName='' password='' email=''>
