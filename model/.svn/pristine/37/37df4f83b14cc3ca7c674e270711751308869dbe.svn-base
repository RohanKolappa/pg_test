          			<title>Set File BypassAutoStorageRules Response</title>
          			<programlisting>
            			<![CDATA[
							<SetFileBypassAutoStorageRules>
								<File NID='' title='' fileType='' streamType='' size=''
									playlength='' presence='' state='' dateCreated='' dateModified=''
									readOnly='' mediaGroupId='' bypassAutoStorageRules=''>
									<description />
									<Groups>
										<Tag name='' value='' />
									</Groups>
									<MetaData>
										<Tag Name=''>Value</Tag>
									</MetaData>
									<Dir dirNID='' dirTitle='' />
								</File>
							</SetFileBypassAutoStorageRules>
						]]>
