	<Streamer>
		<Name>Generic<!--Applies to all streamer objects--></Name>
		<CreateParameters>
			<Parameter>
				<Name>id</Name>
				<Description>A unique object ID</Description>
				<Type>Integer</Type>
			</Parameter>
		</CreateParameters>
	</Streamer>
    <Streamer>
        <Name>V2DDecoderPort</Name>
    	<Type>Sender</Type>
    	<CreateParameters>
    		<Parameter>
    			<Name>dstport</Name>
    			<Description>Server Port</Description>
				<Type>Integer</Type>
				<Default>None</Default>
    		</Parameter>
    		<Parameter>
    			<Name>mcastttl</Name>
    			<Description>Time to Live</Description>
				<Type>Integer</Type>
				<Default>1</Default>
    		</Parameter>
    		<Parameter>
    			<Name>mcastaddr</Name>
    			<Description>Multicast Address</Description>
				<Type>String</Type>
				<Default>None</Default>
    		</Parameter>
    		<Parameter>
    			<Name>maxbitrate</Name>
    			<Description>Max output bandwidth</Description>
				<Type>Integer</Type>
				<Default>100 mbps</Default>
    		</Parameter>    		
    		<Parameter>
    			<Name>v2dtx</Name>
    			<Description>Whether we are on real TX</Description>
				<Type>Boolean</Type>
				<Default>0</Default>
    		</Parameter>    		
    		<Parameter>
    			<Name>listener</Name>
    			<Description>Start a new connection if requested</Description>
				<Type>Boolean</Type>
				<Default>0</Default>
    		</Parameter>    		
    		<Parameter>
    			<Name>policycheck</Name>
    			<Description>Authorize connection based on policy file</Description>
				<Type>Boolean</Type>
				<Default>1</Default>
    		</Parameter>    		
    		<Parameter>
    			<Name>policyfile</Name>
    			<Description>Authorize connection based on policy file</Description>
				<Type>String</Type>
				<Default>None</Default>
    		</Parameter>    		
    		<Parameter>
    			<Name>interface</Name>
    			<Description>Interface to use for multicast</Description>
				<Type>Integer</Type>
				<Default>0</Default>
    		</Parameter>    		
    	</CreateParameters>
    	<UpdateParameters>
    		<Parameter>
    			<Name>port</Name>
    			<Description>Must be same as the one passed at create time</Description>
				<Type>Integer</Type>
				<Default>None</Default>
    		</Parameter>
    		<Parameter>
    			<Name>sockfd</Name>
    			<Description>Audio/video/control socket for HTTP connection</Description>
				<Type>Integer</Type>
				<Default>None</Default>
    		</Parameter>
    		<Parameter>
    			<Name>tcpcontrolfd</Name>
    			<Description>Control socket</Description>
				<Type>Integer</Type>
				<Default>None</Default>
    		</Parameter>
    		<Parameter>
    			<Name>bandwidth</Name>
    			<Description>Maximum output bandwidth</Description>
				<Type>Integer</Type>
				<Default>100 Mbps</Default>
    		</Parameter>
    		<Parameter>
    			<Name>enablesrd</Name>
    			<Description>Toggle SRD On/Off</Description>
				<Type>Boolean</Type>
				<Default>True</Default>
    		</Parameter>
    		<Parameter>
    			<Name>srdpolicy</Name>
    			<Description>SRD Stereo frames policy</Description>
				<Type>String</Type>
				<Default>Source</Default>
    		</Parameter>
    		<Parameter>
    			<Name>debugslices</Name>
    			<Description>Send SRD Debug slices</Description>
				<Type>Boolean</Type>
				<Default>False</Default>
    		</Parameter>
    		<Parameter>
    			<Name>markslices</Name>
    			<Description>Send Marker slices</Description>
				<Type>Boolean</Type>
				<Default>False</Default>
    		</Parameter>
    		<Parameter>
    			<Name>disconnectall</Name>
    			<Description>Disconnect the client</Description>
				<Type>Boolean</Type>
				<Default>None</Default>
    		</Parameter>
    	</UpdateParameters>
    	<NotificationList>
    	</NotificationList>
    </Streamer>
    <Streamer>
    	<Name>DMSMediaPort</Name>
