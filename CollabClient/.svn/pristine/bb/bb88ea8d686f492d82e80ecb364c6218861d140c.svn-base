  <body>
  <script>
	 Ext.application({
	  name:'MyApp',
	  launch: function() {
		var floatingPanel = Ext.create('Ext.panel.Panel', {
		  //height: 200,
		  //width: 500,
		  draggable: true,
		  floating: true,
		  //left: 0,
		  //top: 0,
		  renderTo:Ext.getBody(),
		  items: [

			{
			  xtype: 'barcoplayer',
			  itemId: 'barcoplayer1',
			  title: 'Embedded Player'
			}
		  ]
		});

	  }
	});
  </script>
  </body>
