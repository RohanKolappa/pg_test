/*------------------------------------------
	Alerts and Messages 
------------------------------------------*/
.sessionMessagePanelPopup {
  position: absolute;
  left: 0px;
  top: 0px;
  z-index: 4294967295 !important;
  right: 0;
  padding: 7px;
  border: 1px solid transparent;
  margin-left: auto;
  margin-right: auto;
  transition: top 300ms cubic-bezier(0.17, 0.04, 0.03, 0.94);
overflow: hidden;
}

.sessionMessagePanelPopup span {
	display:inline-block;
	margin:0;
	padding:0;
  	font-size: 1.0em;
}
.sessionMessagePanelPopup span:first-child {
	margin-right: 20px;
  	font-size: 1.1em;
	font-weight:700;
}

.messagePanel-success {
background-color: #3cbc7b;
background-image: url(data:image/png;base64,);
background-repeat: no-repeat;
background-position:7px 10px;
color: #fff;
padding-left: 35px;
padding-right: 35px;
text-align:left;
white-space:normal;
}

