<!DOCTYPE html>
<html>
  <head>
    <title>Sench Touch Test Container for Barco Embedded Player</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width">
    <link rel="stylesheet" href="../../lib/touch/resources/css/sencha-touch.css" type="text/css">
	<script src="../../lib/touch/sencha-touch-all-debug.js"></script>
	
    <link rel="stylesheet" href="../barco/player.css" type="text/css">
	
    <script src="../jquery/jquery-1.7.1.js"></script>
    <script src="../jwplayer6/jwplayer.js" type="text/javascript"></script>
	
	<script src="./BarcoPlayer.js"></script> <!-- jQuery Container -->
    <script src="./TouchBarcoPlayer.js"></script> <!-- Sencha Touch Container -->
   
  </head>
  <body>
  <script>
  Ext.application({
	  launch: function() {
		var floatingPanel = Ext.create('Ext.Panel', {
		  height: 419,
		  width: 652,
		  draggable: true,
		  floating: true,
		  left: 0,
		  top: 0,
		  showAnimation: 'pop',
		  hideAnimation: 'hide',
		  items: [
			{
			  xtype: 'toolbar',
			  docked: 'top',
			  title: 'Embedded Player'
			},
			{
			  xtype: 'barcoplayer',
			  itemId: 'barcoplayer1'
			}
		  ]
		});
		var fsPanel = Ext.create('Ext.Panel', {
		  fullscreen: true,
		  style: 'background-color: #CCF;',
		  html: 'Full-screen Panel'
		});
		Ext.Viewport.add(floatingPanel);
	  }
	});

  </script>
  </body>
</html>
