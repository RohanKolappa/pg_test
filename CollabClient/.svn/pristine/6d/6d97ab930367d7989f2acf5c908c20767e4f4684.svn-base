/*---------------------------------------
Display Panel
---------------------------------------*/
.display-panel-container {
	background: #eaeaea;
	margin-left: 10px;
}
.display-item-container {
	position: relative;
	width: 400px;
	height: 308px;
	display: block;
	margin: 10px 20px 50px 50px;
	-webkit-box-shadow: 0px 2px 3px 0px rgba(0,0,0,0.4);
	-moz-box-shadow: 0px 2px 3px 0px rgba(0,0,0,0.4);
	box-shadow: 0px 2px 3px 0px rgba(0,0,0,0.4);
}
.display-item-title {
	position:relative;
	width: 400px;
	height: 30px;
	/*background: #c8c9cb;*/
	font-family: Arial, Helvetica, sans-serif;
	color: #e4e4e4; /*#3d444a*/
	font-size: 16px;
	padding: 5px 0px 0px 0px; /*10px*/
	margin: 0;
}
.display-item-thumb-container {
	position: relative;
	width: 400px;
	height: 230px;
	background-color: #676d73;
}
.display-item-thumb {
	position:relative;  /* Must use Relative positioning because child elements are absolute positioned */
	height: 230px;
	margin: 0px;
	background: #676d73;
	/* background-image: url("../image/displaythumb.png"); */
	background-position: center;
	background-repeat: no-repeat;
}
.display-window-selected {
	border: solid 3px #0f98e3;
	background-color: rgba(15, 152, 227, 0.20);	
}

.display-item-controls {
	position: relative;
	width: 400px;
	height: 48px;
	padding: 8px 0 4px 0; margin: 0;
	background: #d3d4d5;
}
/* Window control buttons */
