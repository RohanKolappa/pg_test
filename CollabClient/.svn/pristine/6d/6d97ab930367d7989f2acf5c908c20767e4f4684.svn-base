/*---------------------------------------
Button Styles
---------------------------------------*/
.sourceTabOff {
	padding:2px 10px 0px 10px;
	border-radius: 0;
	border: none;
	background: #2f3438;
	color: #949494;
	font-weight:normal;
	font-size:14px;
	text-align:center;
}
.sourceTabOn {
	padding:0px 10px 0px 10px;
	border-radius: 0;
	border: none;
	border-top: solid 3px #ed1c24;
	background: #2f3438;
	color: #fff;
	font-weight:normal;
	font-size:14px;
	text-align:center;
}
.sourceTabPressed {
	border-radius: 0;
