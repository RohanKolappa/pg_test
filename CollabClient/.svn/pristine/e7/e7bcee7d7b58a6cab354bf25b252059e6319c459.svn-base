.bookmarks-panel {
  width:100%;
  text-align:center;
  height: 20px;
}
.bookmarks-container {
  position:relative;
  display:inline-block;
  height:20px;
  width:500px;
  border:solid 1px blue;
}
.bookmark-off {
  position:absolute;
  width:6px;
  height:12px;
  background-image: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAYAAAAMCAYAAABBV8wuAAAAGXRFWHRTb2Z0d2FyZQBBZG9iZSBJbWFnZVJlYWR5ccllPAAAADtJREFUeNpinDlz5n8GLICJAQcYohJHgJgdiMWQJV4DcXRaWtovIAaxTcBiwCDx+P//PwMyBopFAQQYAE0eGpvS72qIAAAAAElFTkSuQmCC);
  top:0px;
}

.bookmark-on {
  position:absolute;
  width:6px;
  height:12px;
  background-image: url(data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAYAAAAMCAYAAABBV8wuAAAAGXRFWHRTb2Z0d2FyZQBBZG9iZSBJbWFnZVJlYWR5ccllPAAAADtJREFUeNpi1Fnw7D8DFsDEgAMMUYkjQMwOxGLIEq+BOPpyvOQvIAaxTcBiwCDx+P//PwMyBopFAQQYADQsGoK9MFL6AAAAAElFTkSuQmCC);
  top:0px;
}
.bookmarkPopup {
  position: absolute;
  display: block;
  top: 25px;
  left: 50px;
  background-color: #333333;
  padding: 3px 4px 4px 4px;
  z-index: 1;
}

.bookmarkPopup input {
  font-size: 0.75em;
  background-color: #111111;
  border: 1px solid;
  margin: 2px;
  color: #cccccc
}
