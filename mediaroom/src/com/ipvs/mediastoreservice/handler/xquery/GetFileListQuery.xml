declare function local:getList()
as element()*
{
	let $list1 := 
		for $b in index-scan('IPVSis_colnids_default', 'default.mediastreamfileresourcelist', 'EQ')/..   
			where $b _XPATH_
			order by $b 
			return $b
	return $list1
};

declare function local:formatItem($b as element()*) {
	let $item := <MediaStreamFileResource NID='{ data($b/@NID) }' 
			   >
		      <UUID>{ data($b//UUID) }</UUID>
		      </MediaStreamFileResource>
	return $item
};






let $list1 := local:getList()
let $list3 := for $b in  $list1 return local:formatItem($b)
return <FileList>{$list3}</FileList>
