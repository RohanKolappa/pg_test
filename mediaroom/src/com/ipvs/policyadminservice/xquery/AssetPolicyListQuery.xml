(: #include TargetObjectLibQuery.xml :)
let $assetPolicyRuleAfterConvertingintoXpath := ( 
	    for $policyRule in index-scan('IPVSis_colnids_default', 'default.assetadminpolicyrulelist', 'EQ')/..
	    where $policyRule/valid = "true"
	    order by $policyRule/order	
		return <AssetAdminPolicyRule> {local:getPolicyRule($policyRule,$policyRule/AssetAdminObjectFilterSet/AssetAdminObjectFilterNID,$policyRule/AssetAdminRoleSet)}</AssetAdminPolicyRule> 
	)
return <AssetAdminPolicyRuleList>{$assetPolicyRuleAfterConvertingintoXpath}</AssetAdminPolicyRuleList>

	
