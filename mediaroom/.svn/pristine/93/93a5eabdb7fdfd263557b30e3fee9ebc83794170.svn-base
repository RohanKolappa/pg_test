	<File_Name_Schemes>
		<!--
			File_Name_Scheme
			testXmlFile name  creation rule:
			AssetResource_BASE_ALL_ADD.xml =  object: attrb=name  +  Base_all or Partial_base: attrb=name   +  testXmlFile: attrb=suffix (_ADD.xml)
			xml target folder= objectXmlFolder
			object_ATTRB_NAME = 1			
			( e.g. - AssetResource, DMS-120-Device , V2D-Tx-Device, V2D-Rx-Device... etc )
			data_type__ATTRB_SUFFIX =2		
			(e.g  Base_all name="base1" suffix="_BASE_ALL" or Partial_base name="basePart1" suffix="_BASE_PART" or Partial_base name="basePart1" suffix="_BASE_PART")
			testXmlFile_ATTRB_SUFFIX = 3		
			( e.g. - _ADD , _DELETE_NID , _UPDATE_NID, _GET_LIST_FROM_COUNT..etc )
			testXmlFile_ATTRB_SUFFIX_AS_FILE_NAME = 4	
		-->
		<File_Name_Scheme name="File_Name_Scheme1" scheme="123" remark="object_ATTRB_NAME + data_type__ATTRB_SUFFIX + testXmlFile_ATTRB_SUFFIX " />
		<File_Name_Scheme name="File_Name_Scheme2" scheme="321" remark="testXmlFile_ATTRB_SUFFIX + data_type__ATTRB_SUFFIX + object_ATTRB_NAME" />
		<File_Name_Scheme name="File_Name_Scheme3" scheme="231" remark="data_type__ATTRB_SUFFIX + testXmlFile_ATTRB_SUFFIX + object_ATTRB_NAME" />
		<File_Name_Scheme name="File_Name_Scheme4" scheme="132" remark="object_ATTRB_NAME + testXmlFile_ATTRB_SUFFIX + data_type__ATTRB_SUFFIX " />
		<File_Name_Scheme name="File_Name_Scheme5" scheme="213" remark="data_type__ATTRB_SUFFIX + object_ATTRB_NAME + testXmlFile_ATTRB_SUFFIX  " />
		<File_Name_Scheme name="File_Name_Scheme6" scheme="312" remark="testXmlFile_ATTRB_SUFFIX+ object_ATTRB_NAME +  data_type__ATTRB_SUFFIX  " />
		<File_Name_Scheme name="File_Name_Scheme7" scheme="4" remark="object_ATTRB_NAME + data_type__ATTRB_SUFFIX + testXmlFile_ATTRB_SUFFIX " />
	</File_Name_Schemes>
<!--
		createTestXmlFiles : 
		requestAction:
		ACTION_GET_REQUEST = "GetRequest";
		ACTION_REPLACE_REQUEST = "ReplaceRequest";
		ACTION_DELETE_REQUEST = "DeleteRequest";
		ACTION_ADD_REQUEST = "AddRequest";
		
		enableFilter for:
		FILTER_NID = 'A';
    	FILTER_OPERATION_DATA = 'G';
    	FILTER_ORDER_BY_XPATH = 'B';
    	FILTER_WHERE_XPATH = 'C';
    	FILTER_CURSOR = 'E';
    	FILTER_SELECT_XML = 'F';    
    	FILTER_CHILD_XPATH_CONTEXT = 'H';    	
		File_Name_Scheme and suffix used to create name of the file :
		<File_Name_Scheme name="File_Name_Scheme1"/>
	-->
	<createTestXmlFiles name="createXmls">
		 <!-- add  -->
		<testXmlFile requestAction="AddRequest" File_Name_Scheme="File_Name_Scheme1" suffix="add" enableFilter="AG" />
		 
		<!-- replace -->
		<testXmlFile requestAction="ReplaceRequest" File_Name_Scheme="File_Name_Scheme1" suffix="replace" enableFilter="AG" />

		<!-- delete -->
		<testXmlFile requestAction="DeleteRequest" File_Name_Scheme="File_Name_Scheme1" suffix="delete" enableFilter="A" />
		
		<!-- get -->
		<testXmlFile requestAction="GetRequest" File_Name_Scheme="File_Name_Scheme1" suffix="get" enableFilter="ABE" />
		
		<!-- readquery -->
		<testXmlFile requestAction="ReadQueryRequest" File_Name_Scheme="File_Name_Scheme1" suffix="readquery" enableFilter="" />	
		
	</createTestXmlFiles>
	
	<createTestXmlFiles name="createXmls_All">
		 <!-- add  -->
		<testXmlFile requestAction="AddRequest" File_Name_Scheme="File_Name_Scheme1" suffix="add" enableFilter="AG" />
		<testXmlFile requestAction="AddRequest" File_Name_Scheme="File_Name_Scheme1" suffix="addChild" enableFilter="AGH" />
		 
		<!-- replace -->
		<testXmlFile requestAction="ReplaceRequest" File_Name_Scheme="File_Name_Scheme1" suffix="replace" enableFilter="AG" />
		<testXmlFile requestAction="ReplaceRequest" File_Name_Scheme="File_Name_Scheme1" suffix="replaceChild" enableFilter="AGH" />

		<!-- delete -->
		<testXmlFile requestAction="DeleteRequest" File_Name_Scheme="File_Name_Scheme1" suffix="delete" enableFilter="A" />
		<testXmlFile requestAction="DeleteRequest" File_Name_Scheme="File_Name_Scheme1" suffix="deleteChild" enableFilter="AH" />
		
		<!-- get -->
		<testXmlFile requestAction="GetRequest" File_Name_Scheme="File_Name_Scheme1" suffix="get" enableFilter="AB" />
		<testXmlFile requestAction="GetRequest" File_Name_Scheme="File_Name_Scheme1" suffix="getSelect" enableFilter="ABCF" />
		<testXmlFile requestAction="GetRequest" File_Name_Scheme="File_Name_Scheme1" suffix="getList" enableFilter="ABE" />
		<testXmlFile requestAction="GetRequest" File_Name_Scheme="File_Name_Scheme1" suffix="getListSelect" enableFilter="ABCEF" />
		
		<!-- readquery -->
		<testXmlFile requestAction="ReadQueryRequest" File_Name_Scheme="File_Name_Scheme1" suffix="readquery" enableFilter="" />	
		
	</createTestXmlFiles>

 	<!-- ///////////////////////////////////////////////////  SAMPLE ///////////////////////////////////////////////////-->
	<object settings="AMSettings" name="AssetResource" enable='false'>
		<requestXML suffix="_SAMPLE_" createTestXmlFiles="createXmls">
			<whereXPathList>
				<xpath>//AssetResource/Info</xpath>
			</whereXPathList>
			<childXpath></childXpath>
			<get>
				<sortByXpathList>
					<xpath>./Info/Title</xpath>
				</sortByXpathList>
				<sortOrder>
					ascending
				</sortOrder>
				<selectXml>
					<SelectXmlResult>
						<xpath>//info</xpath>
					</SelectXmlResult>
				</selectXml>
				<cursor>
					<startFrom>0</startFrom>
					<countToFetch>200</countToFetch>
				</cursor>
			</get>
			<addReplace>
				<parentContextXpath/>
				<operationDataXpathList></operationDataXpathList>
			</addReplace>
			<readquery>
				<!--  todo: file/resource/text -->
				<queryType>resource</queryType>
				<query>/com/ipvs/test/xml/AssetResource.xquery</query>
			</readquery>				
		</requestXML>
		<requestXML suffix="_SAMPLE_PARTIAL" createTestXmlFiles="createXmls">
			<whereXPathList>
				<xpath>//AssetResource/Info/Tag</xpath>
			</whereXPathList>
			<childXpath></childXpath>
			<get>
				<sortByXpathList>
					<xpath>./Info/Title</xpath>
				</sortByXpathList>
				<sortOrder>
					ascending
				</sortOrder>
				<selectXml>
					 <SelectXmlResult>
						{$VAR//info},
						{substring($VAR//info,'sunnyvale')},
						{count($VAR//Title)}
					</SelectXmlResult>
				</selectXml>
				<cursor>
					<startFrom>0</startFrom>
					<countToFetch>-1</countToFetch>
				</cursor>
			</get>
			<addReplace>
				<parentContextXpath>//AssetResource/Info</parentContextXpath>
				<operationDataXpathList>
				</operationDataXpathList>
			</addReplace>
			<readquery>
				<!--  todo: file/resource/text -->
				<queryType>test</queryType>
				<query>for $b in #OBJ_AssetResource_OBJ#/info return $b/Title</query>
			</readquery>	
		</requestXML>
	</object>
	
