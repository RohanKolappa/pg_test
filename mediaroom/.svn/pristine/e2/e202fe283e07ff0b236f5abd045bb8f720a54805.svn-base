  var srcPath = "";
  var callerPath = parent.location.href;
  if ((callerPath.indexOf("IPVS_AdminUI") != -1) || (callerPath.indexOf("STX_ClientUI") != -1)) {
    srcPath = "../IPVS-Common/mediaroom";
  }
  else {
    srcPath = "JSXAPPS/IPVS-Common/mediaroom";
  }
  if (getClientType() == "INSTALLED") {
    parent.document.getElementById("applet").src = srcPath + "/applet_16.html";
  }
  else if (getClientType() == "WEB") {
    parent.document.getElementById("applet").src = srcPath + "/applet_14.html";
  }
  function getClientType() {
    if (window.location.href.indexOf("file:") != -1 || window.location.href.indexOf("http://localhost") != -1) {
      return "INSTALLED";
    }
    else if (window.location.href.indexOf("http://") != -1) {
      return "WEB";
    }
   
  }
 
