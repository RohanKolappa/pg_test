<?xml version="1.0" encoding="UTF-8"?>
<xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">
	<xsl:template match="//SetDeviceRoute">
		<NetworkElementsConfig>
			<RouteTableConfig>
				<RouteEntry>
					<xsl:attribute name="Dirty"> 
					      <xsl:choose>
							<xsl:when test="./@operation!=''">
									<xsl:value-of select="./@operation" />
								</xsl:when>
								<xsl:otherwise>
									<xsl:text>ADD</xsl:text>
								</xsl:otherwise>
							</xsl:choose> 						    							 
						 </xsl:attribute>
					<xsl:attribute name="ListIndex"> 
							<xsl:choose>
							<xsl:when test="./@listIndex!=''">
									<xsl:value-of select="./@listIndex" />
								</xsl:when>
								<xsl:otherwise>
									<xsl:text>0</xsl:text>
								</xsl:otherwise>
							</xsl:choose> 
						 </xsl:attribute>
					<Destination>
						<xsl:choose>
							<xsl:when test="./@destination!=''">
								<xsl:value-of select="./@destination" />
							</xsl:when>
							<xsl:otherwise>
								<xsl:text>false</xsl:text>
							</xsl:otherwise>
						</xsl:choose>
					</Destination>
					<Netmask>
						<xsl:choose>
							<xsl:when test="./@netmask!=''">
								<xsl:value-of select="./@netmask" />
							</xsl:when>
							<xsl:otherwise>
								<xsl:text>1</xsl:text>
							</xsl:otherwise>
						</xsl:choose>
					</Netmask>

					<Gateway>
						<xsl:choose>
							<xsl:when test="./@gateway!=''">
								<xsl:value-of select="./@gateway" />
							</xsl:when>
							<xsl:otherwise>
								<xsl:text>false</xsl:text>
							</xsl:otherwise>
						</xsl:choose>
					</Gateway>
				</RouteEntry>
			</RouteTableConfig>
		</NetworkElementsConfig>
	</xsl:template>
</xsl:stylesheet>