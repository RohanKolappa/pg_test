    	type1("PiP", 2, 1, "1920", "1080", "PiP_Canvas_1920x1080"),
    	type2("PiP", 2, 1, "2560", "1600", "PiP_Canvas_2560x1600"),
    	type3("PiP", 2, 1, "3840", "1200", "PiP_Canvas_3840x1200"),
    	type4("PaP", 2, 1, "1920", "1080", "2x1_Canvas_1920x1080"),
     	type5("PaP", 2, 1, "2560", "1600", "2x1_Canvas_2560x1600"),
    	type6("PaP", 2, 1, "3840", "1200", "2x1_Canvas_3840x1200"),
    	type7("TwoxTwo", 2, 2, "1920", "1080", "2x2_Canvas_1920x1080"),
    	type8("TwoxTwo", 2, 2, "2560", "1600", "2x2_Canvas_2560x1600"),
    	type9("TwoxTwo", 2, 2, "3840", "1200", "2x2_Canvas_3840x1200"),
    	type10("ThreexTwo", 3, 2, "1920", "1080", "3x2_Canvas_1920x1080"),
    	type11("ThreexTwo", 3, 2, "2560", "1600", "3x2_Canvas_2560x1600"),
    	type12("ThreexTwo", 3, 2, "3840", "1200", "3x2_Canvas_3840x1200");
    	
    	private String displaySize;
    	private int numColumns;
    	private int numRows;
    	private String hres;
    	private String vres;
    	private String title;
    	    	
    	private DisplayTypeList(String displaySize, int numColumns, int numRows, String hres, String vres, String title) {
    		this.displaySize = displaySize;
    		this.numColumns = numColumns;
    		this.numRows = numRows;
    		this.hres = hres;
    		this.vres = vres;
    		this.title = title;
    	}
    	
    	public String getDisplaySize() {
    		return displaySize;
  	   	}
    	
    	public int getNumColumns() {
    		return numColumns;
    	}
    	
    	public int getNumRows() {
    		return numRows;
    	}
    	
    	public String getHres() {
    		return hres;
    	}
    	
    	public String getVres() {
    		return vres;
  	   	}
    	
    	public String getTitle() {
    		return title;
    	}
    	   
    	public int getNumWindows() {
    		return numRows*numColumns;
    	}
    	
    	public String getVpos(int winIndex) {
    		if (displaySize.equals("PiP")) {
    			return "0";
    		}

    		// pap winsperrow 2, numRows 1:  winindex 1->0, 2-> 0
           // 2x2, 2, 2: winindex  1->0, 2->0, 3->50%, 4->50%
           // 3x2, 3, 2: winindex 1 -> 0, 2->0, 3->0, 4->50%, 5->50%, 6->50%
    		int rowNum = (winIndex-1)/numColumns;
    		int eachRowRes = 100/numRows;
   	    	int ret = rowNum * eachRowRes;
   	    	return String.valueOf(ret) + "%";
    	}

    	public String getHpos(int winIndex) {
    		if (displaySize.equals("PiP")) {
    			if (winIndex == 1)
    				return "0";
    			else 
    				return "60%"; 
    		}

    		// pap winsperRow 2, numRows 1:  winindex 1->0, 2-> 50%
           // 2x2, 2, 2: winindex  1->0, 2->50%, 3->0, 4->50%
           // 3x2, 3, 2: winindex 1 -> 0, 2->33%, 3->66%, 4->0%, 5->33%, 6->66%
    		int colNum = (winIndex-1)%numColumns;
    		int eachColRes = 100/numColumns;
   	    	int ret = colNum * eachColRes;
   	    	return String.valueOf(ret) + "%";   
    	}

    	public String getVres(int winIndex) {
    		if (displaySize.equals("PiP")) {
    			if (winIndex == 1)
    				return "100%";
    			else
    				return "40%";
    		}

    		// pap winsPerRow 2, numRows 1:  winIndex 0 -> 100, 1 -> 100
           // 2x2, 2, 2: winindex  1->50%, 2->50%, 3->50%, 4-> 50%
           // 3x2, 3, 2: winindex 1 -> 50%, 2->50, 3->50, 4->50%, 5->50%, 6->50%
    		int eachRowRes = 100/numRows;
    		return String.valueOf(eachRowRes) + "%";
    	}

    	public String getHres(int winIndex) {
    		if (displaySize.equals("PiP")) {
    			if (winIndex == 1) 
    				return "100%";
    			else
    				return "40%";
    		}

    		// pap winsPerRow 2, numRows 1:  winindex 1->50, 2-> 50
           // 2x2, 2, 2: winindex  1->50%, 2->50%, 3->50%, 4->50%
           // 3x2, 3, 2: winindex 1 ->33%, 2->33%, 3->33%, 4->33%, 5->33%, 6->33%
    		int eachColRes = 100/numColumns;
    		return String.valueOf(eachColRes) + "%";
    	}
    	
	}
    
