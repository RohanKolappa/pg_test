<project basedir="." default="build" name="mediaroom">
    <property environment="env"/>
    <property name="ECLIPSE_HOME" value="../../../../opt/eclipse"/>
    <property name="debuglevel" value="source,lines,vars"/>
    <property name="ipvsclienttarget" value="1.4"/>
    <property name="ipvsclientsource" value="1.4"/>
    <property name="ipvsclientbootclasspath" value="3rdpartylib/rt.jar"/>
    <path id="ipvsclientmediaroom.classpath">
        <pathelement location="bin"/>
        <pathelement location="3rdpartylib/commons-beanutils.jar"/>
        <pathelement location="3rdpartylib/commons-collections-3.2.jar"/>
        <pathelement location="3rdpartylib/commons-lang-2.3.jar"/>
        <pathelement location="3rdpartylib/commons-logging-1.1.jar"/>
        <pathelement location="3rdpartylib/commons-logging-adapters-1.1.jar"/>
        <pathelement location="3rdpartylib/commons-logging-api-1.1.jar"/>
        <pathelement location="3rdpartylib/log4j-1.2.15.jar"/>
        <pathelement location="3rdpartylib/smack-1.4.jar"/>
        <pathelement location="3rdpartylib/smackx-1.4.jar"/>
        <pathelement location="3rdpartylib/su-smack-1.2.jar"/>
        <pathelement location="3rdpartylib/jaxen.jar"/>
        <pathelement location="3rdpartylib/dom4j.jar"/>
        <pathelement location="3rdpartylib/plugin.jar"/>        
        <pathelement location="3rdpartylib/xpp3.jar"/>
    </path>

    <target name="clean">
        <delete dir="bin"/>
    </target>
    <target depends="clean" name="cleanall"/>
    <target name="init">
        <mkdir dir="bin"/>
    </target>

    <target depends="clean,init,build-subprojects,ipvsclientbuild-project,ipvsclientbuild-release" name="build"/>
    <target name="build-subprojects"/>
    <target name="ipvsclientbuild-project">
        <echo message="${ant.project.name}: ${ant.file}" level="info"/>
        <javac debug="true" debuglevel="${debuglevel}" destdir="bin" source="${ipvsclientsource}" target="${ipvsclienttarget}" bootclasspath="${ipvsbootclasspath}" sourcepath="" srcdir="src">
			<include name="com/ipvs/app/BaseApplet.java"/>
			<include name="com/ipvs/xmpp/client/smack/packets/IPVSMessageExtension.java"/>
			<include name="com/ipvs/xmpp/client/smack/packets/SessionIQ.java"/>
			<include name="com/ipvs/app/IPVSClientUtils.java"/>
			<include name="com/ipvs/app/IPVSMessageHandler.java"/>
			<include name="com/ipvs/app/IPVSClient.java"/>
			<include name="com/ipvs/app/IPVSClientApplet.java"/>
			<include name="org/dom4j/io/Dom4jXMPPPacketReader.java"/>
            <classpath refid="ipvsclientmediaroom.classpath"/>
        </javac>
        <javac debug="true" debuglevel="${debuglevel}" destdir="bin" source="${source}" target="${target}" bootclasspath="${bootclasspath}">
            <src path="3rdpartylib"/>
            <classpath refid="ipvsclientmediaroom.classpath"/>
        </javac>
    </target>
	
    <target name="ipvsclientbuild-release" description="ipvsclient.jar">
	    <mkdir dir="dist"/>
	    <jar destfile="dist/ipvsclient.jar" manifest="src/MANIFEST.MF">
	        <metainf dir="src/META-INF" />
	        <fileset dir="bin">
				<include name="com/ipvs/app/AppletLog.class"/>
				<include name="com/ipvs/app/BaseApplet.class"/>
				<include name="com/ipvs/xmpp/client/smack/packets/IPVSMessageExtension.class"/>
				<include name="com/ipvs/xmpp/client/smack/packets/SessionIQ.class"/>
				<include name="com/ipvs/app/IPVSClientUtils.class"/>
				<include name="com/ipvs/app/IPVSMessageHandler.class"/>
				<include name="com/ipvs/app/IPVSClient.class"/>
				<include name="com/ipvs/app/IPVSClientApplet.class"/>
				<include name="com/ipvs/app/JSNotifier.class"/>
				<include name="com/ipvs/app/WrapperJSMethod.class"/>
				<include name="com/ipvs/app/Notification.class"/>
	        	<include name="org/dom4j/io/Dom4jXMPPPacketReader.class"/>
		</fileset>
	    </jar>
    </target>
