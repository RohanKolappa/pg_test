<?xml version="1.0" encoding="UTF-8"?>
<cs:cspecExtension xmlns:cs="http://www.eclipse.org/buckminster/CSpec-1.0">
	<cs:actions>
		<cs:public name="jar" actor="ant">
			<cs:actorProperties>
				<cs:property key="buildFile" value="build.xml" />
				<cs:property key="targets" value="build" />
			</cs:actorProperties>
			<cs:properties>
				<cs:property key="output" value="${buckminster.output}/pkg/lib/com.barco.nimbus.model.jar" />
			</cs:properties>
			<cs:products alias="action.output" base="pkg/">
				<cs:path path="lib/com.barco.nimbus.model.jar" />
			</cs:products>
		</cs:public>
	</cs:actions>
</cs:cspecExtension>
