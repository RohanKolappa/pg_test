table.float {
float: left;
}
body, td { font : 85% "Trebuchet MS", "Lucida Grande", Verdana, Arial, sans-serif; }

#ipvsPlayer {
	border: solid 2px #003366;
}
.smallHeader {
	height: 24px;
	background-color: #23417D;
	color: #fff;
	padding: 4px;
}
.screen {
	border: solid 3px #003366;
	background-color: #000;
	}
.title {
 	font: bold 14px Verdana, Geneva, Arial, Helvetica, sans-serif;
 	color: #fff;
}
.smallHeaderOuter {
	border-top: solid 2px #003366;
	border-right: solid 2px #003366;
	border-left: solid 2px #003366;
	border-bottom: none;
}
.smallHeaderInner {
	border: solid 1px #7592BB;
	background-image: url(images/headerBG.jpg);
	padding: 4px;
	background-repeat: repeat-x;
	background-position: left top;
	color: #fff;
	font-size: 120%;
	}
.playerControls {
	border-top: solid 0px #003366;
	border-right: solid 0px #7592BB;
	border-left: solid 0px #7592BB;
	border-bottom: solid 0px #003366;
	background-color: #37609B;
	padding: 1px;
	color: #fff;
	}
#statusDisplayContainer {
	height: 14px;
	background-color: #000033;
}
#statusDisplay {
	float: left;
	height: 14px;
	background-color: #000033;
	font-size: 10px;
	color: #00FDB8;
	font-family: verdana;
	font-weight: normal;
	padding-left: 4px;
}
#clientURL {
	height: 14px;
	background-color: #000033;
}
#networkStatus {
	float: right;
	text-align: right;
	height: 14px;
	background-color: #000033;
	color: #00FDB8;
	font: 10px verdana, arial, sans-serif;
}
.clientURL {
	height: 14px;
	background-color: #000033;
	font-size: 10px;
	color: Aqua;
	font-family: verdana;
	font-weight: normal;
	text-align: right;
	padding-right: 4px;
	border: 0px;
}
.playerSettingsOuter {
	border-top: none;
	border-right: solid 2px #003366;
	border-left: solid 2px #003366;
	border-bottom: solid 3px #003366;
}
.playerSettingsInner {
	background-color: #92A9CA;
	padding-top: 4px;
	padding-bottom: 10px;
	padding-left: 5px;
	padding-right: 5px;
	color: #fff;
	font-weight: normal;
	}
.inputText {
	font-size: 80%;
	color: #000033;
	}
input.textBox {
	border: solid 1px #003366;
	background-color: #567BB1;
	height: 18px;
	}
input.textBoxBasic {
 	font: 10px Verdana, Geneva, Arial, Helvetica, sans-serif;
 	color: #102132;
	background-color: #fff;
	height: 18px;
	border: solid 1px gray;
	}
.playerSettings {
 background-color: #CEDCE9;
 color: #000000;
 border: 1px solid #E3ECF5;
 border-collapse: collapse;
}

.playerSettings td {
 border: 1px solid #D7E5F2;
 padding-left: 4px;
}
.headercell {
 color: #fff;
 padding-left: 5px;
 background-color: #4A71A9;
}
.labelcell {
 height: 28px;
 font: 11px Verdana, Geneva, Arial, Helvetica, sans-serif;
 color: #000033;
 text-align: right;
 padding-left: 10px;
}
.labelcellhd {
 height: 28px;
 font: 11px Verdana, Geneva, Arial, Helvetica, sans-serif;
 color: #000033;
 text-align: middle;
 padding-left: 3px;
}

.fieldcell {
 height: 28px;
 color: #000000;
 text-align: right;
 margin-right: 0px;
 padding-right: 0px;
}
.bodyText {
	color: #fff;
}
.statusText {
	color: #CAFE5C;
	padding-left: 5px;
	background-color: #000033;
}

.launchButton {
	height: 20px;
	BORDER-LEFT: solid 1px white;
	border-right: solid 1px black;
	border-top: solid 1px white;
	border-bottom: solid 1px black;
	padding-left: 15px;
	padding-right: 15px;
	padding-top: 2px;
	background-image: url(Images/launchBtnBG.jpg);
	color: white;
	text-decoration: none;
	background-repeat: repeat-x;
}

.header4 {
 font: 11px Verdana, Geneva, Arial, Helvetica, sans-serif;
 color: #fff;
 padding-left: 5px;
 background-color: #4A71A9;
}
.T2 {
 font: 11px Verdana, Geneva, Arial, Helvetica, sans-serif;
 color: #000033;
}