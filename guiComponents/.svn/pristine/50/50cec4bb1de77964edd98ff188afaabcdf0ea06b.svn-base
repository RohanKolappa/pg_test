/*==========================================
	Windows
==========================================*/
.window {
	position:relative;
	display:block;
	min-height:100px;
	min-width:100px;
	padding: 20px 5px;
	background: #262b2f;
}
.window-header {
	position:absolute;
	top:0;
	height: 20px;
}
.window-footer {

}

/*------------------------------------------
	Modal
------------------------------------------*/
.modal-window {
	position:absolute;
	left:0;right:0;
	top: 0;bottom:0;
	background: rgba(35,37,39, 0.9);
}
.modal-bg {
	background: #2f3438;
	opacity: 0.25;
}


