/* generic input styling - override this if you need specific styling */
.search-form-input {
	background: #313436;
	border: 1px solid #606264;
	color: #e5e5e5;
	padding: 2px 6px;
	height: 25px;
  font-family: "Dax", Verdana, Helvetica, Arial, sans-serif !important;
  font-size: 14px !important;
	-webkit-appearance: none;
	border-radius: 2px;
	-webkit-box-shadow: none;
	box-shadow: none;
	-webkit-transition: border .25s linear, color .25s linear, background-color .25s linear;
	transition: border .25s linear, color .25s linear, background-color .25s linear;
}


.form-group input[type="text"],
.form-group input[type="password"],
.form-group input[type="number"],
.form-group input[type="email"],
.form-group textarea,
.form-group select {
	background: #313436;
	border: 1px solid #606264;
	color: #e5e5e5;
	padding: 2px 6px;
	height: 32px;
	-webkit-appearance: none;
	border-radius: 2px;
	-webkit-box-shadow: none;
	box-shadow: none;
	-webkit-transition: border .25s linear, color .25s linear, background-color .25s linear;
	transition: border .25s linear, color .25s linear, background-color .25s linear;
}
.form-group input.error {
  border: solid 2px #f75454;
}
.form-group input[disabled],
.form-group input[readonly] {
  background: #3b4146;
  border: solid 1px #4a4e52;
  color: #888;
}
.form-group-small input[type="text"],
.form-group-small input[type="password"],
.form-group-small input[type="number"],
.form-group-small input[type="email"],
.form-group-small textarea,
.form-group-small select {
	background: #313436;
	border: 1px solid #606264;
	color: #e5e5e5;
	padding: 2px 6px;
	height: 24px;
	-webkit-appearance: none;
	border-radius: 2px;
	-webkit-box-shadow: none;
	box-shadow: none;
	-webkit-transition: border .25s linear, color .25s linear, background-color .25s linear;
	transition: border .25s linear, color .25s linear, background-color .25s linear;
}
