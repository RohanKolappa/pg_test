/*==========================================
	Globals
==========================================*/
html, body {
  	background: #3d444a;
	font-family: "Dax", Verdana, Helvetica, Arial, sans-serif;
	color: #e4e4e4;
	
	/* IE Scrollbars */
	scrollbar-base-color: #52585d;
	scrollbar-base-color: #52585d;
	scrollbar-3dlight-color: #3c4143;
	scrollbar-highlight-color: #3c4143;
	scrollbar-track-color: #3c4143;
	scrollbar-arrow-color: black;
	scrollbar-shadow-color: #3c4143;
	scrollbar-darkshadow-color: #3c4143;
}


/*==========================================
	Base Colors
==========================================*/
.bg {
	background: #3d444a;
}


/*==========================================
	Scrollbar Dark Theme
==========================================*/
::-webkit-scrollbar {
    width: 10px;
}
 
/* Track */
::-webkit-scrollbar-track {
    -webkit-border-radius: 0px;
    border-radius: 0px;
	background: rgb(43,48,52);
	background: #484e51;
	background: #3c4143;
}
 
/* Handle */
::-webkit-scrollbar-thumb {
    -webkit-border-radius: 0px;
    border-radius: 0px;
    background: rgb(82,88,93); 
}
::-webkit-scrollbar-thumb:window-inactive {
	background: rgba(53,59,64); 
}




