/*==========================================
	Tabs
==========================================*/
.sourceTabOff {
	padding:2px 10px 0px 10px;
	border-radius: 0;
	border: none;
	background: #2f3438;
	color: #949494;
	font-weight:normal;
	font-size:14px;
	text-align:center;
}
.sourceTabOn {
	padding:0px 10px 0px 10px;
	border-radius: 0;
	border: none;
	border-top: solid 3px #ed1c24;
	background: #2f3438;
	color: #fff;
	font-weight:normal;
	font-size:14px;
	text-align:center;
}
/* jquery tab overrides */
#tabs.ui-state-default, #tabs.ui-widget-content .ui-state-default, #tabs.ui-widget-header .ui-state-default {
	padding:4px 10px 0px 10px;
	border-radius: 0;
	border: none;
	background: #2f3438;
	color: #949494;
	font-weight:normal;
	font-size:18px;
	text-align:center;
	margin-bottom: 0px;
	margin-right: -5px;
  margin-top: 0px;
	}
#tabs.ui-state-default span{
	color: #949494;
}

#tabs.ui-state-active, #tabs.ui-widget-content .ui-state-active, #tabs.ui-widget-header .ui-state-active {
  background-image: none;
  padding:1px 10px 0px 10px;
	border-radius: 0;
	border: none;
	border-top: solid 3px #ed1c24;
	background: #262b2f;
	color: #fff;
	font-weight:normal;
	font-size:18px;
	text-align:center;
	/* margin-bottom: 1px; */
  margin-top: 0px;
}
#tabs.ui-state-active span{
	color: #e4e4e4;
}


#live-source-walls-container.ui-tabs-panel {
	padding: 0;
}
#recorded-source-walls-container.ui-tabs-panel {
	padding: 0;
}
