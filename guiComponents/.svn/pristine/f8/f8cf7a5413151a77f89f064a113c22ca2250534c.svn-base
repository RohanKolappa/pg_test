  display: table-cell; 
  vertical-align: middle; 
  text-align: center;
  line-height:1;
  font-size:inherit;
}
.sc-con-ok {
  color: #95c11f;
  width:40px;
  text-align:center;
}
i.stretch {
    display:inline-block;
    transform:scale(3,1);
