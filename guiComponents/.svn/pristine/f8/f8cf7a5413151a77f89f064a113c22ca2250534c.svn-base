}

/*------------------------------------------
	Create Sessions
------------------------------------------*/
.session-container {
	position:absolute;
	top: 0px;
	bottom: 10px;
	left: 0px;
	right: 0px;
}

.session-save {
	position:absolute;
	top: 0px;
	bottom: 10px;
	left: 0px;
	right: 0px;
}
.session-connections {
  position:absolute;
  top:0px;
  left:0px;
  right:0px;
  bottom:0px;
}
.session-connections-panels {
  position:absolute;
  top:0px;
  left:0px;
  right:0px;
  bottom:80px;
}
.session-connections-footer {
  position:absolute;
  height:80px;
  left:0px;
  right:0px;
  bottom:0px;
  text-align:center;
  line-height: 80px;
}
.session-connections-panels .left { 
	left: 10px !important;
	width: 300px !important; 
  transition: all .3s;
}
.session-connections-panels .center { 
	left: 320px !important;
	right: 320px !important;
  transition: all .3s;  
}
.session-connections-panels .right { 
	right: 10px !important;
	width: 300px!important; 
  transition: all .3s;  
}
.session-connections-panels .header-panel {
  height:40px;
  line-height:40px;
}
.session-connections-panels .body-panel {
  top:40px;
  bottom:0px;
}
.session-connections-panels .display-settings-btn {
  display:block;
  position:absolute;
  right:10px;
  top: 0px;
  line-height:inherit;
  z-index:99;
}
.session-container .source-menu-btn {
  display:none;
}
.session-container .display-menu-btn {
  display:none;
}
.add-session-sources,
.add-session-displays {
  line-height:50px;
  width: 90%;
  height: 100px;
  border-radius: 4px;
  text-align:center;
  margin: 50% auto;
  -webkit-font-smoothing: subpixel-antialiased;
  -webkit-transition: 0.25s linear;
  transition: 0.25s linear;
}
.add-session-sources:hover,
.add-session-displays:hover {
  background: #485157;
  cursor: pointer;
}

.session-source-panel {
  position:absolute;
  left: -20px !important;
  width:0px !important;
  bottom:20px !important;
  z-index:9999;
  transition: width .3s;
}
.session-source-panel.open {
  left:0px !important;
  width: 100% !important;
}
.session-display-panel {
  position:absolute;
  right: -20px !important;
  width:0px !important;
  bottom:20px !important;
  z-index:9999;
  transition: width .3s;
}
.session-display-panel.open {
  right:0px !important;
  width: 100% !important;
}


