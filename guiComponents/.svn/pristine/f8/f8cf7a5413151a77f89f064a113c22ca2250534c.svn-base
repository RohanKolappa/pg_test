/*------ Radial Graphs ------*/

.graph {
  position: relative;
  width: 300px;
  height: 300px;
  border-radius: 50%;
  background: #ccc;
}

.graph-mask {
  position: absolute;
  width: 270px;
  height: 270px;
  background: #fff url('http://i.imgur.com/xamVdNK.jpg') center center no-repeat;
  border-radius: 50%;
  top: 50%;
  left: 50%;
  z-index: 1000;
  -webkit-transform: translate3d(-50%, -50%, 0);
}

.slice-content {
  position: absolute;
  width: 300px;
  height: 300px;
  border-radius: 50%;
  clip: rect(0px, 150px, 300px, 0px);
}

.slice {
  position: absolute;
  width: 300px;
  height: 300px;
  border-radius: 50%;
  clip: rect(0px, 300px, 300px, 150px)
}

.slice-mask {
  -webkit-transform: rotate(45deg);
}
.slice-mask .slice-content {
  background: #fff;
  -webkit-transform: rotate(135deg);
}

.slice-fill {
  -webkit-transform: rotate(180deg);
}
.slice-fill .slice-content {
  background: #3ea8b5;
  -webkit-animation: bouncy 2s 0s infinite alternate forwards;
}

@-webkit-keyframes bouncy {
  0% {-webkit-transform: rotate(0deg);}
  100% {-webkit-transform: rotate(180deg);}
}

