/*------------------------------------------
	Session Monitor
------------------------------------------*/
.sc-connection {
  display:block;
  min-width:800px;
  border: 0px solid #485157;
  -webkit-box-shadow: 0px 2px 3px 0px rgba(0,0,0,0.25);
  -moz-box-shadow: 0px 2px 3px 0px rgba(0,0,0,0.25);
  box-shadow: 1px 2px 1px 0px rgba(0,0,0,0.25);
  font-family: "Dax-Light", Verdana, Helvetica, Arial, sans-serif;
  position: relative;
  background: #373c41;
  color: #e4e4e4;
  border-radius: 2px;
  margin:20px 20px 40px 20px;
  padding:10px;
}
.sc-label {
  margin:5px 10px 15px 10px;
}
.sc-label h1 {
  display:inline-block;
  width: 150px;
  margin:0;
  line-height:14px;
  font-size:16px;
  color: #949494;
  text-align:left;
}
.sc-connection p {
  font-size: 14px;
  color: #e4e4e4;
}
.sc-type {
  display:inline-block;
  height:30px;
  margin:10px;
  vertical-align:top;
}

.sc-status {
  display:block;
  height:20px;
  margin:2px 0 0 93px;
  vertical-align:top;
}
.sc-status p {
  font-size: 14px;
  color: #e4e4e4;
}
.sc-con {
  display:inline-block;
  height:30px;
  margin:5px 10px;
  vertical-align:top;
}
.sc-con-type {
  float:left;
  height:30px;
  line-height:30px;
  width:130px;
  color: #e4e4e4;
  margin:5px 10px;
  overflow:hidden;
  white-space:nowrap;
  text-overflow: ellipsis;
  text-align:left;
}
.sc-con-src,
.sc-con-ok,
.sc-con-error,
.sc-con-dst,
.sc-con-detail {
  height:30px;
  line-height:30px;
  vertical-align:middle;
  font-size:14px;
  float:left;
  margin-left:4px;
}
.sc-con-src,
.sc-con-dst {
  max-width:150px;
  overflow:hidden;
  white-space:nowrap;
