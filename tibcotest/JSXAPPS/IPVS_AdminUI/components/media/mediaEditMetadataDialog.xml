<serialization jsxversion="3.9" xmlns="urn:tibco.com/v3.0">
  <name><![CDATA[Dialog]]></name>
  <icon><![CDATA[images/prototypes/dialog.gif]]></icon>
  <description><![CDATA[Dialog box.  (Set Modal property for a modal dialog).]]></description>
  <onBeforeDeserialize></onBeforeDeserialize>
  <onAfterDeserialize></onAfterDeserialize>
  <object type="jsx3.gui.Dialog">
    <variants jsxwidth="640" jsxheight="480" jsxzindex="5000" jsxwindowstate="1" jsxleft="0" jsxtop="0"></variants>
    <strings jsxname="DialogEditMetaData"></strings>
    <dynamics jsxbgcolor="@Solid Medium"></dynamics>
    <object type="jsx3.gui.WindowBar">
      <variants></variants>
      <strings jsxname="windowBar" jsxtext="Edit Metadata" jsxcolor="#fff;"></strings>
      <dynamics jsxfontsize="BaseText" jsxbg="Dialog Titlebar" jsxbgcolor="@Solid Shadow"></dynamics>
      <object type="jsx3.gui.ToolbarButton">
        <variants></variants>
        <strings jsxname="btnMinimize"></strings>
        <dynamics jsximage="@Min Icon" jsxtip="jsx3.gui.Dialog.min"></dynamics>
        <events jsxexecute="this.getAncestorOfType(jsx3.gui.Dialog).doToggleState();"></events>
      </object>
      <object type="jsx3.gui.ToolbarButton">
        <variants></variants>
        <strings jsxname="btnMaximize"></strings>
        <dynamics jsximage="@Max Icon" jsxtip="jsx3.gui.Dialog.max"></dynamics>
        <events jsxexecute="this.getAncestorOfType(jsx3.gui.Dialog).doMaximize(this);"></events>
      </object>
      <object type="jsx3.gui.ToolbarButton">
        <variants></variants>
        <strings jsxname="btnDialogCloseX"></strings>
        <dynamics jsximage="@Close Icon" jsxtip="jsx3.gui.Dialog.close"></dynamics>
        <events jsxexecute="this.getAncestorOfType(jsx3.gui.Dialog).doClose();"></events>
      </object>
    </object>
    <object type="jsx3.gui.LayoutGrid">
      <variants jsxrelativeposition="0" jsxleft="0" jsxtop="0" jsxoverflow="2"></variants>
      <strings jsxname="layout (--)" jsxrows="*,40" jsxwidth="100%" jsxheight="100%"></strings>
      <dynamics jsxbgcolor="P3"></dynamics>
      <object type="jsx3.gui.Block">
        <variants jsxoverflow="1"></variants>
        <strings jsxname="blkBody" jsxwidth="100%" jsxheight="100%" jsxstyleoverride="overflow-x:hidden"></strings>
        <dynamics jsxbgcolor="Dialog BG" jsxborder="@Outset Bottom"></dynamics>
        <object type="jsx3.gui.Block">
          <variants jsxoverflow="3" jsxheight="40" jsxrelativeposition="1" jsxzindex="2"></variants>
          <strings jsxname="blkErrorMsg" jsxwidth="100%" jsxdisplay="none" jsxborder="border: solid 1px #ff0000" jsxpadding="10 10 10 10"></strings>
          <dynamics jsxbgcolor="P4BG"></dynamics>
          <object type="jsx3.gui.Image">
            <variants jsxoverflow="2" jsxrelativeposition="1"></variants>
            <strings jsxname="imgErrorIcon" jsxsrc="../../addins/ipvsCommon/images/icons/icon_error.png" jsxmargin="0 10 0 0"></strings>
          </object>
          <object type="jsx3.gui.Block">
            <variants jsxrelativeposition="1" jsxoverflow="3" jsxfontsize="12"></variants>
            <strings jsxname="lblMetadataErrorMsg" jsxcolor="#ff0000"></strings>
          </object>
        </object>
        <object type="jsx3.gui.Block">
          <variants jsxrelativeposition="1" jsxoverflow="3" jsxheight="30" jsxfontsize="14"></variants>
          <strings jsxname="lblMetadataSource" jsxpadding="10 10 10 10"></strings>
        </object>
        <object type="jsx3.gui.Block">
          <variants jsxoverflow="3" jsxrelativeposition="1" jsxheight="100"></variants>
          <strings jsxname="blkMetadataList" jsxwidth="100%" jsxdisplay=""></strings>
          <object type="jsx3.gui.Block">
            <variants jsxoverflow="3" jsxheight="50"></variants>
            <strings jsxname="blkMetadataItemOrg" jsxwidth="90%" jsxrelativeposition="1" jsxpadding="10 0 0 0" jsxdisplay="none"></strings>
            <object type="jsx3.gui.Block">
              <variants jsxrelativeposition="1" jsxoverflow="3" jsxwidth="200" jsxfontsize="14"></variants>
              <strings jsxname="lblMetadataName" jsxtext="Metadata Label" jsxtextalign="left" jsxpadding="6 10 0 20"></strings>
            </object>
            <object type="jsx3.gui.TextBox">
              <variants jsxwidth="300" jsxheight="30" jsxtype="1"></variants>
              <strings jsxname="txtMetadataValue" jsxmargin="0 4 0 10" jsxoverflow="auto" jsxvalue=""></strings>
            </object>
          </object>
        </object>
      </object>
      <object type="jsx3.gui.Block">
        <variants jsxoverflow="1"></variants>
        <strings jsxname="blkFooter" jsxwidth="100%" jsxheight="100%" jsxtextalign="right"></strings>
        <dynamics jsxborder="@Outset Top" jsxbgcolor="Dialog Footer"></dynamics>
        <object type="jsx3.gui.Button">
          <variants jsxindex="0" jsxheight="22" jsxwidth="60" jsxfontsize="11"></variants>
          <strings jsxname="btnEditMetadataCancel" jsxtext="Cancel" jsxmargin="8 8 0 0" jsxtextalign="center" jsxbgcolor="" jsxpadding="padding-top: 3px;" jsxborder="" jsxclassname="btnOk" jsxdisplay="none"></strings>
          <events jsxexecute="closeDialog(this);"></events>
        </object>
        <object type="jsx3.gui.Button">
          <variants jsxindex="0" jsxheight="22" jsxwidth="60" jsxfontsize="11"></variants>
          <strings jsxname="btnEditMetadataDone" jsxtext="Done" jsxmargin="8 8 0 0" jsxtextalign="center" jsxbgcolor="" jsxpadding="padding-top: 3px;" jsxborder="" jsxclassname="btnOk" jsxdisplay="none"></strings>
          <events jsxexecute=""></events>
        </object>
      </object>
    </object>
  </object>
</serialization>