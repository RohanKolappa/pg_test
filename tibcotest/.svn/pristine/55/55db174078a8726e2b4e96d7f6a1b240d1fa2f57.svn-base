<?xml version="1.0" ?>

<data jsxid="jsxroot">

  <record jsxid="jsxblur" jsxtext="Blur"
      jsxtip="Fires after this whiteboard loses focus.">
    <record jsxid="this" type="$class$" jsxtext="the object that publishes this event."/>
    <record jsxid="objEVENT" type="jsx3.gui.Event" jsxtext="the browser event that triggers this event."/>
  </record>

  <record jsxid="jsxfocus" jsxtext="Focus"
      jsxtip="Fires after this whiteboard gains focus.">
    <record jsxid="this" type="$class$" jsxtext="the object that publishes this event."/>
    <record jsxid="objEVENT" type="jsx3.gui.Event" jsxtext="the browser event that triggers this event."/>
  </record>

  <record jsxid="jsxmenu" jsxtext="Menu"
      jsxtip="Fires after the user right-clicks this whiteboard and before the bound context menu displays. Canceling this event prevents the menu from displaying."
      vetoable="1">
    <record jsxid="this" type="$class$" jsxtext="the object that publishes this event."/>
    <record jsxid="objEVENT" type="jsx3.gui.Event" jsxtext="the browser event that triggers this event."/>
    <record jsxid="objMENU" type="jsx3.gui.Menu" vartype="inout" jsxtext="the menu to display."
        jsxreturntext="use this field to override the default menu with an instance of &lt;code&gt;jsx3.gui.Menu&lt;/code&gt;."/>
  </record>

  <record jsxid="jsxdestroy" jsxtext="Destroy" always="1"
      jsxtip="Fires after this whiteboard is removed from the DOM.">
    <record jsxid="this" type="$class$" jsxtext="the object that publishes this event."/>
    <record jsxid="objPARENT" type="jsx3.app.Model"
        jsxtext="the parent of this whiteboard before it is removed from the DOM."/>
  </record>

</data>
