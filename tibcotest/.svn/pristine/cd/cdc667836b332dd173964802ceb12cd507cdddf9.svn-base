/* Panels
-----------------------------------------------------------*/

#JSX .header {
    background-color: #1e6081;
    background-image:url(images/panels/headerBG.png);
    background-position: left center;
    background-repeat: repeat-x;
    border-bottom:solid 1px #acacac;
    color: #fff;
}

#JSX .roomOff {
    background-color: #177db0;
    border: 1px solid #5c9fc1;
}
#JSX .roomOver {
    border: 1px solid #6dadcd;
    background-color: #3e8cb9;
    color: #ffffff;
}
#JSX .roomOn {
    border: 1px solid #d0e2ed;
    background-color: #5fa4cc;
    color: #ffffff;
}

#JSX .sourceOff {
    background-color: #e6e6e6;
    border-left: 1px solid #ffffff;
    border-top: 1px solid #ffffff; 
   border-right: 1px solid #d7d8d8;
    border-bottom: 1px solid #d7d8d8; 
}
#JSX .sourceOn {
    background-color: #6699cc;
    border-left: 1px solid #666;
    border-top: 1px solid #666; 
    border-right: 1px solid #ffffff;
    border-bottom: 1px solid #ffffff;
    color: #ffffff;
}

#JSX .destinationOff {
    background-color: #e6e6e6;
    border-left: 1px solid #ffffff;
    border-top: 1px solid #ffffff; 
   border-right: 1px solid #d7d8d8;
    border-bottom: 1px solid #d7d8d8; 
}

#JSX .destinationOn {
    background-color: #6699cc;
    border-left: 1px solid #666666;
    border-top: 1px solid #666666; 
    border-right: 1px solid #ffffff;
    border-bottom: 1px solid #ffffff;
    color: #ffffff;
}


#JSX .controllerPanel {
    background-color: #000000;
    border-top: solid 0px #000000; 
    border-bottom: 1px solid #acacac; 
    border-right: 1px solid #acacac; 
    border-left: 1px solid #acacac;
}

#JSX .glass {
    background-image:url(images/panels/glass.png);
    background-position: top;
    background-repeat: repeat-x;
}

#JSX .titlebar {
    background-color: #ccc;
    background-repeat: repeat-x;
    border-bottom:solid 1px #777777;
}





/* Buttons
------------------------------------------------------------------*/


#JSX .btnBlackGlass {
    background-image:url(images/buttons/blackGlass.png);
    background-position: center;
    background-repeat: repeat-x;
    border: solid 1px #acacac;
    color: #fff;
    cursor: hand;
}

#JSX .btnGrayGlass, #JSX .btnDefault, #JSX .btnOk, #JSX .btnCancel {
    background-image:url(images/buttons/grayGlass.png);
    background-position: center;
    background-repeat: repeat-x;
    border: solid 1px #acacac;
    color: #fff;
    cursor: hand;
}

#JSX .btnBlueGlass {
    background-color: #678ea2;
    background-image:url(images/buttons/blueGlass.png);
    background-position: center;
    background-repeat: repeat-x;
    border: solid 1px #91bbd1;
    color: #fff;
    cursor: hand;
}

#JSX .btnGreenGlass, #JSX .btnAdd {
    background-color: #689839;
    background-image:url(images/buttons/greenGlass.png);
    background-position: center;
    background-repeat: repeat-x;
    border: solid 1px #bcbcbc;
    color: #fff;
    cursor: hand;
}

#JSX .btnRedGlass {
    background-color: #ff0000;
    background-image:url(images/buttons/redGlass.png);
    background-position: center;
    background-repeat: repeat-x;
    border: solid 1px #bcbcbc;
    color: #fff;
    cursor: hand;
}

#JSX .btnOrangeGlass {
    background-color: #ff0000;
    background-image:url(images/buttons/orangeGlass.png);
    background-position: center;
    background-repeat: repeat-x;
    border: solid 1px #bcbcbc;
    color: #fff;
    cursor: hand;
}

#JSX .btnHighlight {
    border: solid 2px #00ff00;
    font-weight:bold;
    font-size: 12px;
}

#JSX .btnDisabled {
    filter:alpha(opacity=50);
    -moz-opacity:0.5;
    -khtml-opacity: 0.5;
    opacity: 0.5;
}


