#include "JBS.Tools.MkDep.Directive.h"

// ============================================================================

using namespace JBS::Tools::MkDep;

// ============================================================================

BFC_PTR_IMPL(Directive)

// ============================================================================

Directive& Directive::operator = (
	const	Directive&		d ) {

	if ( this != &d ) {
		type = d.type;
		expr = d.expr;
		str1 = d.str1;
		str2 = d.str2;
	}

	return *this;

}

