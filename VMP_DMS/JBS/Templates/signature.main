static void doIt() {

	// Do something useful here!

}

// ============================================================================

int main( int argc, char * argv[] ) {

	if ( argc != 1 ) {
		cerr << "Usage: " << argv[ 0 ] << endl;
		return -1;
	}

	try {
		doIt();
	}
	catch ( Exception & e ) {
		cerr << e.what() << endl;
		return -2;
