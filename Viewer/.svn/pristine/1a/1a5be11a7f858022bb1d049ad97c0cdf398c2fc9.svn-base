BODY 
{ 
    scrollbar-base-color: #434343; 
    scrollbar-arrow-color: #434343;
    SCROLLBAR-FACE-COLOR: #666666;
    SCROLLBAR-HIGHLIGHT-COLOR: #434343;
    SCROLLBAR-SHADOW-COLOR: #434343;
    SCROLLBAR-TRACK-COLOR: #434343;
    scrollbar-DarkShadow-Color: #434343;
    SCROLLBAR-3DLIGHT-COLOR: #434343;
    scrollbar-track-color: #434343;

}

#JSX .bg {
    background-color: #0f0f0f;
    background-position: middle center;
    background-repeat: repeat-x;
    background-image: url(images/panels/bg.jpg);
}

#JSX .loginBg {
    background-position: right;
    background-repeat: no-repeat;
    background-image: url(images/panels/loginBg.png);
}

#JSX .loginMask {
    background-repeat: no-repeat;
    background-image: url(images/panels/loginMask.png);
}

#JSX .transparentMask {
    background-image: url(images/panels/transparentMask.png);
}

#JSX .underline {
    border-bottom: solid 1px #aaaaaa;
}

#JSX .sliderBG {
    background-image: url(images/slider/trackBG.png);
}

#JSX .liveOn {
    text-decoration: underline;
    color: #ffffff;
}

#JSX .liveOff {
    text-decoration: none;
    color: #e5e5e5;
}

#JSX .roundBorder {
 -moz-border-radius: 4px;
 border-radius: 4px;
 }


#JSX .errorBg {
        background-image: url(images/panels/errorBg.png);
        background-postion: top;
        background-repeat: repeat-x;
        border:solid 1px #aaaaaa;
        -moz-border-radius: 4px;
        border-radius: 4px;        
}


#JSX .errorBoxClose {
    -webkit-transition: all 0.3s ease-in-out;
    -moz-transition: all 0.3s ease-in-out;
    -o-transition: all 0.3s ease-in-out;
    -ms-transition: all 0.3s ease-in-out;
    transform: translate(0px,-400px);
    -ms-transform: translate(0px,-400px); /* IE 9 */
    -webkit-transform: translate(0px,-400px); /* Safari and Chrome */
    -o-transform: translate(0px,-400px); /* Opera */
    -moz-transform: translate(0px,-400px); /* Firefox */
}
#JSX .errorBoxOpen {
    -webkit-transition: all 0.3s ease-in-out;
    -moz-transition: all 0.3s ease-in-out;
    -o-transition: all 0.3s ease-in-out;
    -ms-transition: all 0.3s ease-in-out;
    transform: translate(0px,0px);
    -ms-transform: translate(0px,0px); /* IE 9 */
    -webkit-transform: translate(0px,0px); /* Safari and Chrome */
    -o-transform: translate(0px,0px); /* Opera */
    -moz-transform: translate(0px,400px); /* Firefox */
}