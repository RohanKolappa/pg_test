

<Menu>

   <Type>config</Type>
   <Title>SampleConfig</Title>
   <Description>Sample Config</Description>
   <SkipDocs>true</SkipDocs>
   <Access>sample</Access>
   <ConfigFile>sample_config</ConfigFile>

   <Items>

      <Item>
        <Name>ipaddress</Name>
        <Type>string</Type>
        <Label>Ip Address</Label>
        <Help>Enter IP Address</Help>
        <FieldLength>17</FieldLength>
        <DefaultValue>0.0.0.0</DefaultValue>
        <Validation>
           <Type>ipaddress</Type>
        </Validation>
      </Item>

      <Item>
        <Name>netmask</Name>
        <Type>string</Type>
        <Label>Netmask</Label>
        <Help>Enter Netmask</Help>
        <FieldLength>17</FieldLength>
        <DefaultValue>255.255.255.0</DefaultValue>
        <Validation>
           <Type>netmask</Type>
        </Validation>
      </Item>

      <Item>
        <Name>enable</Name>
        <Type>boolean</Type>
        <Label>Enable</Label>
        <Help>Enable Interface</Help>
        <FieldLength>4</FieldLength>
        <DefaultValue>Yes</DefaultValue>
      </Item>

      <Item>
        <Name>password</Name>
        <Type>password</Type>
        <Label>Password</Label>
        <Help>Password to Access Database</Help>
        <FieldLength>32</FieldLength>
        <DefaultValue>secret</DefaultValue>
      </Item>

      <Item>
        <Name>video_port</Name>
        <Type>integer</Type>
        <Label>Video Port</Label>
        <Help>Callback Port</Help>
        <FieldLength>5</FieldLength>
        <DefaultValue>5500</DefaultValue>
        <Validation>
           <Type>range</Type>
           <Min>2000</Min>
           <Max>6000</Max>
        </Validation>
      </Item>

      <Item>
        <Name>audio_port</Name>
        <Type>integer</Type>
        <Label>Audio Port</Label>
        <Help>Audio Callback Port</Help>
        <Align>follow</Align>
        <FieldLength>5</FieldLength>
        <DefaultValue>6600</DefaultValue>
        <Validation>
           <Type>range</Type>
           <Min>1000</Min>
           <Max>7000</Max>
        </Validation>
      </Item>

      <Item>
        <Name>file_name</Name>
        <Type>string</Type>
        <Label>File Name</Label>
        <Help>File Name for where to save data</Help>
        <FieldLength>64</FieldLength>
        <DefaultValue>/tmp/file.txt</DefaultValue>
        <Validation>
           <Type>regexp</Type>
           <Value>anynonempty</Value>
        </Validation>
      </Item>

      <Item>
        <Name>bandwidth</Name>
        <Type>bandwidth</Type>
        <Label>Bandwidth</Label>
        <Help>Bandwidth</Help>
        <FieldLength>11</FieldLength>
        <DefaultValue>4.56M</DefaultValue>
      </Item>


      <Item>
        <Name>domain</Name>
        <Type>string</Type>
        <Label>Domain Name</Label>
        <Help>the domain name</Help>
        <FieldLength>32</FieldLength>
        <DefaultValue>xyz.com</DefaultValue>
        <Validation>
           <Type>regexp</Type>
           <Value>password</Value>
        </Validation>
      </Item>

      <Item>
        <Name>options</Name>
        <Type>string</Type>
        <Label>Option</Label>
        <Help>Some Option</Help>
        <FieldLength>9</FieldLength>
        <DefaultValue>This</DefaultValue>
        <Validation>
           <Type>enumeration</Type>
           <Values>"First", "Second", "Third", "Fourth", "Last", "F/L", "Not Used Yet", "A:B:C"</Values>
        </Validation>
      </Item>
      <Item>                 
        <Name>register_0</Name>
        <Type>string</Type>
        <Label>Register 0</Label>
        <Help>No Help Yet.</Help>
        <FieldLength>7</FieldLength>
        <DefaultValue>0xaa</DefaultValue>
        <Validation>
            <Type>hexadecimal</Type>
        </Validation>
      </Item>

   </Items>
</Menu>
