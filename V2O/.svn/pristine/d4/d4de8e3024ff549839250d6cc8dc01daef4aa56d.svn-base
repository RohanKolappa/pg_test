<Menu>
   <Type>config</Type>
   <SubType>normal</SubType>
   <Title>KeyboardConsoleModeSettings</Title>
   <Description>Keyboard Console Mode Settings</Description>
   <Access>keyboard_console_mode_settings</Access>
   <ConfigFile>kbcons</ConfigFile>
   <BoardAccess>false</BoardAccess>

   <Items>

      <Item>
        <Name>esc_char_1</Name>
        <Type>string</Type>
        <Label>Escape Character #1</Label>
        <Help>Set keyboard console mode escape character #1</Help>
        <FieldLength>9</FieldLength>
        <DefaultValue>LCtrl</DefaultValue>
		<Validation>
           <Type>enumeration</Type>
           <Values>"None", "Esc", "Tab", "CapsLock", "LShift", "LCtrl", "LMeta", "LAlt", "RShift", "RCtrl", "RMeta", "RAlt", "Insert", "Delete"</Values>
        </Validation>
      </Item>
	  <Item>
        <Name>esc_char_2</Name>
        <Type>string</Type>
        <Label>Escape Character #2</Label>
        <Help>Set keyboard console mode escape character #2</Help>
        <FieldLength>9</FieldLength>
        <DefaultValue>LCtrl</DefaultValue>
		<Validation>
           <Type>enumeration</Type>
           <Values>"None", "Esc", "Tab", "CapsLock", "LShift", "LCtrl", "LMeta", "LAlt", "RShift", "RCtrl", "RMeta", "RAlt", "Insert", "Delete"</Values>
        </Validation>
      </Item>
	  <Item>
        <Name>esc_char_3</Name>
        <Type>string</Type>
        <Label>Escape Character #3</Label>
        <Help>Set keyboard console mode escape character #3</Help>
        <FieldLength>9</FieldLength>
        <DefaultValue>LCtrl</DefaultValue>
		<Validation>
           <Type>enumeration</Type>
           <Values>"None", "Esc", "Tab", "CapsLock", "LShift", "LCtrl", "LMeta", "LAlt", "RShift", "RCtrl", "RMeta", "RAlt", "Insert", "Delete"</Values>
        </Validation>
      </Item>
	  <Item>
        <Name>esc_char_4</Name>
        <Type>string</Type>
        <Label>Escape Character #4</Label>
        <Help>Set keyboard console mode escape character #2</Help>
        <FieldLength>9</FieldLength>
        <DefaultValue>None</DefaultValue>
		<Validation>
           <Type>enumeration</Type>
           <Values>"None", "Esc", "Tab", "CapsLock", "LShift", "LCtrl", "LMeta", "LAlt", "RShift", "RCtrl", "RMeta", "RAlt", "Insert", "Delete"</Values>
        </Validation>
      </Item>

	  <Item>
        <Name>icon_color</Name>
        <Type>string</Type>
        <Label>Icon Color</Label>
        <Help>Set keyboard console mode icon color</Help>
        <FieldLength>8</FieldLength>
        <DefaultValue>White</DefaultValue>
		<Validation>
           <Type>enumeration</Type>
           <Values>"White", "Gray", "Red", "Green", "Blue", "Yellow", "Magenta", "Cyan"</Values>
        </Validation>
      </Item>

	  <Item>
        <Name>icon_position</Name>
        <Type>string</Type>
        <Label>Icon Position</Label>
        <Help>Set keyboard console mode icon position</Help>
        <FieldLength>12</FieldLength>
        <DefaultValue>TopLeft</DefaultValue>
		<Validation>
           <Type>enumeration</Type>
           <Values>"TopLeft", "TopRight", "BottomLeft", "BottomRight"</Values>
        </Validation>
      </Item>
	  <Item>
        <Name>icon_duration</Name>
        <Type>integer</Type>
        <Label>Icon Duration</Label>
        <Help>Set keyboard console mode icon duration</Help>
        <FieldLength>7</FieldLength>
        <DefaultValue>0</DefaultValue>
		<Validation>
           <Type>range</Type>
		   <Min>0</Min>
		   <Max>999999</Max>
        </Validation>
      </Item>

   </Items>

</Menu>
