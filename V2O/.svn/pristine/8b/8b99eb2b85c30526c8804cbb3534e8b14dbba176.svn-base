# /etc/profile

# System wide environment and startup programs
# Functions and aliases go in /etc/bashrc


TERM=vt100

PATH=/bin:/sbin:/usr/bin:/usr/sbin:/V2O/bin:/V2O/sbin
TERM=vt100

# Set aliases
alias www="watch -n 1"

# Set Shell Prompt
HOSTNAME=`grep HOSTNAME /V2O/config/netconf.txt | sed 's/^HOSTNAME *//'`
PS1="${HOSTNAME} # "


export TERM PATH HOSTNAME PS1


