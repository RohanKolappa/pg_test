<Menu>
   <Type>config</Type>
   <SubType>normal</SubType>
   <Title>TxKBMParameters</Title>
   <Description>Encoder Keyboard/Mouse Parameters</Description>
   <Access>tx_kbm_parm</Access>
   <ConfigFile>txkbmconf</ConfigFile>
   <BoardAccess>true</BoardAccess>

   <Items>

      <Item>
        <Name>keyboard_type</Name>
        <Type>string</Type>
        <Label>Keyboard Type</Label>
        <Help>Select Keyboard Type</Help>
        <FieldLength>4</FieldLength>
        <DefaultValue>PC</DefaultValue>
        <Validation>
           <Type>enumeration</Type>
           <Values>"PC", "SGI"</Values>
        </Validation>
      </Item>

      <Item>
        <Name>mouse_type</Name>
        <Type>string</Type>
        <Label>Mouse Type</Label>
        <Help>Select Mouse Type</Help>
        <FieldLength>6</FieldLength>
        <DefaultValue>IMPS2</DefaultValue>
        <Validation>
           <Type>enumeration</Type>
           <Values>"PS2", "IMPS2"</Values>
        </Validation>
      </Item>

   </Items>
</Menu>
