<Menu>
   <Type>config</Type>
   <Title>ColorParameters</Title>
   <Description>Color Gain and Offset Parameters</Description>
   <Access>color_parm</Access>
   <ConfigFile>colorconf</ConfigFile>
   <BoardAccess>true</BoardAccess>

   <Items>

      <Item>
        <Name>red_gain</Name>
        <Type>integer</Type>
        <Label>Red Gain</Label>
        <Help>Enter red color gain of the video</Help>
        <FieldLength>4</FieldLength>
        <DefaultValue>128</DefaultValue>
        <Validation>
           <Type>range</Type>
           <Min>0</Min>
           <Max>255</Max>
        </Validation>
      </Item>

      <Item>
        <Name>red_offset</Name>
        <Type>integer</Type>
        <Label>Red Offset</Label>
        <Align>follow</Align>
        <Help>Enter red color offset of the video</Help>
        <FieldLength>4</FieldLength>
        <DefaultValue>64</DefaultValue>
        <Validation>
           <Type>range</Type>
           <Min>0</Min>
           <Max>127</Max>
        </Validation>
      </Item>

      <Item>
        <Name>green_gain</Name>
        <Type>integer</Type>
        <Label>Green Gain</Label>
        <Help>Enter green color gain of the video</Help>
        <FieldLength>4</FieldLength>
        <DefaultValue>128</DefaultValue>
        <Validation>
           <Type>range</Type>
           <Min>0</Min>
           <Max>255</Max>
        </Validation>
      </Item>

      <Item>
        <Name>green_offset</Name>
        <Type>integer</Type>
        <Label>Green Offset</Label>
        <Align>follow</Align>
        <Help>Enter green color offset of the video</Help>
        <FieldLength>4</FieldLength>
        <DefaultValue>64</DefaultValue>
        <Validation>
           <Type>range</Type>
           <Min>0</Min>
           <Max>127</Max>
        </Validation>
      </Item>

      <Item>
        <Name>blue_gain</Name>
        <Type>integer</Type>
        <Label>Blue Gain</Label>
        <Help>Enter blue color gain of the video</Help>
        <FieldLength>4</FieldLength>
        <DefaultValue>128</DefaultValue>
        <Validation>
           <Type>range</Type>
           <Min>0</Min>
           <Max>255</Max>
        </Validation>
      </Item>

      <Item>
        <Name>blue_offset</Name>
        <Type>integer</Type>
        <Label>Blue Offset</Label>
        <Align>follow</Align>
        <Help>Enter blue color offset of the video</Help>
        <FieldLength>4</FieldLength>
        <DefaultValue>64</DefaultValue>
        <Validation>
           <Type>range</Type>
           <Min>0</Min>
           <Max>127</Max>
        </Validation>
      </Item>

   </Items>
</Menu>
