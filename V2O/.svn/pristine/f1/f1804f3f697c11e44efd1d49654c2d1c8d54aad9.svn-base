             <Min>0</Min> 
             <Max>2047</Max> 
        </Validation>   
      </Item>

      <Item>
        <Name>pix3x</Name>
        <Type>integer</Type>
        <Label>Pixel 3 X</Label>
        <Help>Pix3 X co-ordinate</Help>
        <FieldLength>7</FieldLength>
        <DefaultValue>0</DefaultValue>
        <Validation> 
             <Type>range</Type> 
             <Min>0</Min> 
             <Max>4095</Max> 
        </Validation>   
      </Item>

      <Item>
        <Name>pix3y</Name>
        <Type>integer</Type>
        <Label>Pixel 3 Y</Label>
        <Help>Pix3 Y co-ordinate</Help>
                <Align>follow</Align>
        <FieldLength>7</FieldLength>
        <DefaultValue>0</DefaultValue>
        <Validation>
             <Type>range</Type> 
             <Min>0</Min> 
             <Max>2047</Max> 
        </Validation>   
