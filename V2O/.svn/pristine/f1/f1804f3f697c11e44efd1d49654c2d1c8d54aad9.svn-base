<Menu>

    <Type>config</Type> 
    <Title>ChromakeyParam</Title>
    <Description>Chromakey Params</Description>
    <Access>chromakey_parm</Access>
    <BoardAccess>true</BoardAccess>
    <ConfigFile>local_chromakey_parm_config</ConfigFile>
    <VerticalOffset>0</VerticalOffset>

    <Items>
        
      <Item>
        <Name>enable</Name>
        <Type>string</Type>
        <Label>Enable</Label>
        <Help>Enabling chromakey functionality</Help>
        <FieldLength>7</FieldLength>
        <DefaultValue>No</DefaultValue>
        <Validation> 
             <Type>enumeration</Type> 
             <Values>"Yes", "No"</Values>
        </Validation> 
      </Item>

      <Item>
        <Name>colorref</Name>
        <Type>string</Type>
        <Label>Color Reference</Label>
        <Help>Reference either value or location </Help>
        <FieldLength>16</FieldLength>
        <DefaultValue>Value</DefaultValue>
        <Validation> 
             <Type>enumeration</Type> 
             <Values>"Value", "Location"</Values> 
        </Validation> 
      </Item>

      <Item>
        <Name>transparency</Name>
        <Type>integer</Type>
        <Label>Transparency</Label>
        <Help>Transparency</Help>
        <FieldLength>7</FieldLength>
        <DefaultValue>0</DefaultValue>
        <Validation> 
              <Type>range</Type> 
              <Min>0</Min> 
             <Max>100</Max> 
        </Validation>   
      </Item>
     
      <Item>
        <Name>redvalue</Name>
        <Type>integer</Type>
        <Label>Red Value</Label>
        <Help>Red color display</Help>
        <FieldLength>7</FieldLength>
        <DefaultValue>0</DefaultValue>
        <Validation> 
             <Type>range</Type> 
             <Min>0</Min> 
             <Max>255</Max> 
        </Validation>   
      </Item>

      <Item>
        <Name>redtolerance</Name>
        <Type>integer</Type>
        <Label>Red Tolerance</Label>
        <Help>Red Tolerence display</Help>
                <Align>follow</Align>
        <FieldLength>7</FieldLength>
        <DefaultValue>0</DefaultValue>
        <Validation> 
             <Type>range</Type> 
             <Min>0</Min> 
             <Max>99</Max> 
        </Validation>   
      </Item>

      <Item>
        <Name>greenvalue</Name>
        <Type>integer</Type>
        <Label>Green Value</Label>
        <Help>Green Value display</Help>
        <FieldLength>7</FieldLength>
        <DefaultValue>0</DefaultValue>
        <Validation> 
             <Type>range</Type> 
             <Min>0</Min> 
             <Max>255</Max> 
        </Validation>   
      </Item>

      <Item>
        <Name>greentolerance</Name>
        <Type>integer</Type>
        <Label>Green Tolerance</Label>
        <Help>Green Tolerance display</Help>
                <Align>follow</Align>
        <FieldLength>7</FieldLength>
        <DefaultValue>0</DefaultValue>
        <Validation> 
             <Type>range</Type> 
             <Min>0</Min> 
             <Max>99</Max> 
        </Validation>   
      </Item>

      <Item>
        <Name>bluevalue</Name>
        <Type>integer</Type>
        <Label>Blue Value</Label>
        <Help>Blue value </Help>
        <FieldLength>7</FieldLength>
        <DefaultValue>0</DefaultValue>
        <Validation> 
             <Type>range</Type> 
             <Min>0</Min> 
             <Max>255</Max> 
        </Validation>   
      </Item>

      <Item>
        <Name>bluetolerance</Name>
        <Type>integer</Type>
        <Label>Blue Tolerance</Label>
        <Help>Blue Tolerance</Help>
                <Align>follow</Align>
        <FieldLength>7</FieldLength>
        <DefaultValue>0</DefaultValue>
        <Validation> 
             <Type>range</Type> 
             <Min>0</Min> 
             <Max>99</Max> 
        </Validation>   
      </Item>

      <Item>
        <Name>pix1x</Name>
        <Type>integer</Type>
        <Label>Pixel 1 X</Label>
        <Help>Pix1 X co-ordinate</Help>
        <FieldLength>7</FieldLength>
        <DefaultValue>0</DefaultValue>
        <Validation> 
             <Type>range</Type> 
             <Min>0</Min> 
             <Max>4095</Max> 
