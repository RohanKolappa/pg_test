      </Item>

      <Item>
        <Name>pix2x</Name>
        <Type>integer</Type>
        <Label>Pixel 2 X</Label>
        <Help>Pix2 X co-ordinate</Help>
        <FieldLength>7</FieldLength>
        <DefaultValue>0</DefaultValue>
        <Validation> 
             <Type>range</Type> 
             <Min>0</Min> 
             <Max>4095</Max> 
        </Validation>   
      </Item>

      <Item>
        <Name>pix2y</Name>
        <Type>integer</Type>
        <Label>Pixel 2 Y</Label>
        <Help>Pix2 Y co-ordinate</Help>
                <Align>follow</Align>
        <FieldLength>7</FieldLength>
        <DefaultValue>0</DefaultValue>
        <Validation> 
