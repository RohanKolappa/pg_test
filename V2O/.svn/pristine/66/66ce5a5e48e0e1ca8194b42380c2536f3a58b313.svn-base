<Menu>
   <Type>config</Type>
   <SubType>immediate</SubType>
   <Title>RxAVBuffers</Title>
   <Description>RX Audio Video Buffers</Description>
   <Access>rx_av_buffers</Access>
   <ConfigFile>rx_av_buffers</ConfigFile>
   <BoardAccess>true</BoardAccess>

   <Items>
      <Item>
        <Name>read_audio_queue_limit</Name>
        <Type>integer</Type>
        <Label>Read Audio Queue Limit</Label>
        <Help>Read Audio Queue Limit</Help>
        <FieldLength>6</FieldLength>
        <DefaultValue>2</DefaultValue>
        <Validation>
           <Type>range</Type>
           <Min>1</Min>
           <Max>1024</Max>
        </Validation>
      </Item>

      <Item>
        <Name>write_video_queue_limit</Name>
        <Type>integer</Type>
        <Label>Write Video Queue Limit</Label>
        <Help>Write Video Queue Limit</Help>
        <FieldLength>6</FieldLength>
        <DefaultValue>16</DefaultValue>
        <Validation>
           <Type>range</Type>
           <Min>1</Min>
           <Max>4000</Max>
        </Validation>
      </Item>

      <Item>
        <Name>write_audio_queue_limit</Name>
        <Type>integer</Type>
        <Label>Write Audio Queue Limit</Label>
        <Help>Write Audio Queue Limit</Help>
        <FieldLength>6</FieldLength>
        <DefaultValue>2</DefaultValue>
        <Validation>
           <Type>range</Type>
           <Min>1</Min>
           <Max>1024</Max>
        </Validation>
      </Item>

   </Items>
</Menu>
