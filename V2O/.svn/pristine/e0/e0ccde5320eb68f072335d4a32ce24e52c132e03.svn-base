<Menu>

    <Type>config</Type>
    <Title>TestMenuOneConfig</Title>
    <Description>Test Config</Description>
    <Access>test_menu_one</Access>
    <ConfigFile>test_menu_one_config</ConfigFile>
    <CliCommandName>undefined</CliCommandName>

    <Items>

      <Item>
        <Name>ipaddress</Name>
        <Type>string</Type>
        <Label>Ip Address</Label>
        <Help>Enter IP Address</Help>
        <FieldLength>17</FieldLength>
        <DefaultValue>0.0.0.0</DefaultValue>
        <Validation>
            <Type>ipaddress</Type>
	    </Validation>
	  </Item>

      <Item>
        <Name>netmask</Name>
        <Type>string</Type>
        <Label>Netmask</Label>
        <Help>Enter Netmask</Help>
        <FieldLength>17</FieldLength>
        <DefaultValue>255.255.255.0</DefaultValue>
        <Validation>
            <Type>netmask</Type>
        </Validation>
      </Item>

      <Item>
        <Name>password</Name>
        <Type>password</Type>
        <Label>Password</Label>
        <Help>Password to Access Database</Help>
        <FieldLength>32</FieldLength>
        <DefaultValue>secret</DefaultValue>
      </Item>

      <Item>
        <Name>float_range</Name>
        <Type>float</Type>
        <Label>Float ex</Label>
        <Help>25.0 to 250.0</Help>
        <FieldLength>7</FieldLength>
        <DefaultValue>25.0</DefaultValue>
        <Validation>
            <Type>range</Type>
            <Min>1.0</Min>
            <Max>250.0</Max>
        </Validation>
      </Item>

      <Item>
        <Name>integer_range</Name>
        <Type>integer</Type>
        <Label>Integer ex </Label>
        <Help>1 to 20</Help>
        <FieldLength>32</FieldLength>
        <DefaultValue>1</DefaultValue>
        <Validation>
            <Type>range</Type>
            <Min>0</Min>
            <Max>20</Max>
        </Validation>
      </Item>

      <Item>
        <Name>enable</Name>
        <Type>boolean</Type>
        <Label>Enable</Label>
        <Help>Enable Interface</Help>
        <FieldLength>4</FieldLength>
        <DefaultValue>Yes</DefaultValue>
      </Item>

      <Item>
        <Name>domain</Name>
        <Type>string</Type>
        <Label>Domain Name</Label>
        <Help>the domain name</Help>
        <FieldLength>11</FieldLength>
        <DefaultValue>xyz.com</DefaultValue>
        <Validation>
            <Type>regexp</Type>
            <Value>password</Value>
        </Validation>
	  </Item>

      <Item>
        <Name>video_port</Name>
        <Type>integer</Type>
        <Label>Video Port</Label>
        <Help>Callback Port</Help>
        <FieldLength>5</FieldLength>
        <DefaultValue>5500</DefaultValue>
        <Validation>
            <Type>range</Type>
            <Min>1000</Min>
            <Max>6000</Max>
        </Validation>
      </Item>

      <Item>
        <Name>audio_port</Name>
        <Type>integer</Type>
        <Label>Audio Port</Label>
        <Help>Audio Callback Port</Help>
        <Align>follow</Align>
        <FieldLength>5</FieldLength>
        <DefaultValue>6600</DefaultValue>
        <Validation>
            <Type>range</Type>
            <Min>2000</Min>
            <Max>7000</Max>
        </Validation>
      </Item>

      <Item>
        <Name>options</Name>
        <Type>string</Type>
        <Label>Option</Label>
        <Help>Some Option</Help>
        <FieldLength>11</FieldLength>
        <DefaultValue>First</DefaultValue>
        <Validation>
            <Type>enumeration</Type>
            <Values>"First", "Second", "Third", "Fourth", "Last"</Values>
        </Validation>
      </Item>

    </Items>

</Menu>
