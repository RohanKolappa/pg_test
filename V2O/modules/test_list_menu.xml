<Menu>

    <Type>list</Type>
    <Title>TestListConfig</Title>
    <Description>Sample List Config</Description>
    <Access>test_list</Access>
    <ConfigFile>test_list_config</ConfigFile>
    <BoardAccess>true</BoardAccess>
    <VerticalOffset>0</VerticalOffset>
    <CliCommandName>undefined</CliCommandName>

    <Items>

      <Item>
        <Name>full_name</Name>
        <Type>string</Type>
        <Label>Full Name :</Label>
        <Help>Name</Help>
        <FieldLength>32</FieldLength>
        <DisplayOnly>true</DisplayOnly>
        <DefaultValue></DefaultValue>
        <Validation>
            <Type>regexp</Type>
            <Value>anynonempty</Value>
        </Validation>
      </Item>

      <Item>
        <Name>first_name</Name>
        <Type>string</Type>
        <Label>First Name :</Label>
        <Help>Name</Help>
        <FieldLength>32</FieldLength>
        <DefaultValue></DefaultValue>
        <Validation>
            <Type>regexp</Type>
            <Value>anynonempty</Value>
        </Validation>
      </Item>


      <Item>
        <Name>last_name</Name>
        <Type>string</Type>
        <Label>Last Name :</Label>
        <Help>Name</Help>
        <FieldLength>32</FieldLength>
        <DefaultValue></DefaultValue>
        <Validation>
            <Type>regexp</Type>
            <Value>anynonempty</Value>
        </Validation>
      </Item>

      <Item>
        <Name>height</Name>
        <Type>float</Type>
        <Label>Height :</Label>
        <Help>Height </Help>
        <FieldLength>5</FieldLength>
        <DefaultValue>5.0</DefaultValue>
        <Validation>
            <Type>range</Type>
            <Min>2.0</Min>
            <Max>8.5</Max>
        </Validation>
      </Item>

      <Item>
        <Name>weight</Name>
        <Type>float</Type>
        <Label>Weight :</Label>
        <Help>Weight </Help>
        <FieldLength>5</FieldLength>
        <DefaultValue></DefaultValue>
        <Validation>
            <Type>range</Type>
            <Min>10</Min>
            <Max>400</Max>
        </Validation>
      </Item>

      <Item>
        <Name>samid</Name>
        <Type>integer</Type>
        <Label>ID :</Label>
        <Help>Sample List ID</Help>
        <FieldLength>4</FieldLength>
        <DefaultValue></DefaultValue>
        <Validation>
            <Type>range</Type>
            <Min>1</Min>
            <Max>169</Max>
        </Validation>
      </Item>

    </Items>
</Menu>
