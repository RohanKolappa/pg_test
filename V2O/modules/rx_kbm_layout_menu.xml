<Menu>
   <Type>config</Type>
   <SubType>normal</SubType>
   <Title>RxKBMLayout</Title>
   <Description>Keyboard Layout</Description>
   <Access>rx_kbm_layout</Access>
   <ConfigFile>rxkbmlayoutconf</ConfigFile>
   <BoardAccess>true</BoardAccess>

   <Items>

      <Item>
        <Name>keyboard_layout</Name>
        <Type>string</Type>
        <Label>Keyboard Layout</Label>
        <Help>Select Keyboard Layout</Help>
        <FieldLength>20</FieldLength>
        <DefaultValue>US</DefaultValue>
		<Validation>
           <Type>enumeration</Type>
           <Values>"US", "German", "French", "Belgian French", "Spanish", "Dutch" , "Italian" , "Portuguese" , "Russian"</Values>
        </Validation>
      </Item>

   </Items>

</Menu>
